        <url>http://opencachetest.garmin.com/#geocache/OX320DR</url>
        <urlname>Hell's Woods Night Cache</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="2830572">
            <groundspeak:name>Hell's Woods Night Cache</groundspeak:name>
