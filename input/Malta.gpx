        <groundspeak:cache archived="False" available="True" id="3572181">
            <groundspeak:name>Casa Ipollito</groundspeak:name>
            <groundspeak:placed_by>Gremxul</groundspeak:placed_by>
            <groundspeak:owner id="88923">Gremxul</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
