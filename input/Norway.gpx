        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2272568">
            <groundspeak:name>Skiens høyeste cache - Turtips.info #3</groundspeak:name>
            <groundspeak:placed_by>Turtips.info</groundspeak:placed_by>
            <groundspeak:owner id="29345">Turtips.info</groundspeak:owner>
