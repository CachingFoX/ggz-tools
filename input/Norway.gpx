        <url>http://opencachetest.garmin.com/#geocache/OX2JVVV</url>
        <urlname>God Jul</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2409098">
            <groundspeak:name>God Jul</groundspeak:name>
            <groundspeak:placed_by>Reodor</groundspeak:placed_by>
