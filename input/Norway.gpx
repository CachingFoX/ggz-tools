        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="2882106">
            <groundspeak:name>Tonningsgate</groundspeak:name>
            <groundspeak:placed_by>Slogen</groundspeak:placed_by>
