        <groundspeak:cache archived="False" available="True" id="1174268">
            <groundspeak:name>[CnK] Il ponte sul naviglio [Milano curiosa]</groundspeak:name>
            <groundspeak:placed_by>cnk-geocaching</groundspeak:placed_by>
            <groundspeak:owner id="123219">cnk-geocaching</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
