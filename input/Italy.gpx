        <desc>S.S.52 Carnica by LzaEla, Traditional Cache(3.5/3.5/3.0)</desc>
        <url>http://opencachetest.garmin.com/#geocache/OX2C87</url>
        <urlname>S.S.52 Carnica</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="71369">
            <groundspeak:name>S.S.52 Carnica</groundspeak:name>
