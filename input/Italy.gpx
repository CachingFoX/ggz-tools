        <groundspeak:cache archived="False" available="True" id="654798">
            <groundspeak:name>Monti Trentini - -Luis Trenker (Cima Vezzena)</groundspeak:name>
            <groundspeak:placed_by>Kingfisherteam</groundspeak:placed_by>
