        <urlname>Playa la posa del obispo</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2135503">
            <groundspeak:name>Playa la posa del obispo</groundspeak:name>
            <groundspeak:placed_by>ZEQ5</groundspeak:placed_by>
            <groundspeak:owner id="8977">ZEQ5</groundspeak:owner>
