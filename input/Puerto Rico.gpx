        <groundspeak:cache archived="False" available="True" id="2176326">
            <groundspeak:name>Plaza de los proceres de Barceloneta</groundspeak:name>
            <groundspeak:placed_by>ZEQ5</groundspeak:placed_by>
            <groundspeak:owner id="8977">ZEQ5</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
