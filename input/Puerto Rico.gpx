        <url>http://opencachetest.garmin.com/#geocache/OX2JVZX</url>
        <urlname>Expreso a Japon</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2409224">
            <groundspeak:name>Expreso a Japon</groundspeak:name>
            <groundspeak:placed_by>JAAS666</groundspeak:placed_by>
            <groundspeak:owner id="7409">JAAS666</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
