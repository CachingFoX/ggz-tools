        <desc>Riga Aviation museum by neviens, Virtual Cache(1.1/1.5/3.2)</desc>
        <url>http://opencachetest.garmin.com/#geocache/OXZTRTC</url>
        <urlname>Riga Aviation museum</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Virtual Cache</type>
        <groundspeak:cache archived="False" available="True" id="28474256">
            <groundspeak:name>Riga Aviation museum</groundspeak:name>
            <groundspeak:placed_by>neviens</groundspeak:placed_by>
            <groundspeak:owner id="15436">neviens</groundspeak:owner>
