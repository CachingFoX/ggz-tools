        <groundspeak:cache archived="False" available="True" id="2341147">
            <groundspeak:name>les pénitents noirs</groundspeak:name>
            <groundspeak:placed_by>triboulet</groundspeak:placed_by>
            <groundspeak:owner id="1535">triboulet</groundspeak:owner>
