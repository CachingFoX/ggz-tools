        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2703756">
            <groundspeak:name>L'Axe de la Terre</groundspeak:name>
            <groundspeak:placed_by>lefaune</groundspeak:placed_by>
            <groundspeak:owner id="67705">lefaune</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
