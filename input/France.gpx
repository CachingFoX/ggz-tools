        <groundspeak:cache archived="False" available="True" id="1476557">
            <groundspeak:name>Il était une foi....des platanes !</groundspeak:name>
            <groundspeak:placed_by>FIL67</groundspeak:placed_by>
            <groundspeak:owner id="42998">FIL67</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
