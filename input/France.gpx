        <groundspeak:cache archived="False" available="True" id="2264048">
            <groundspeak:name>Voie verte des planeurs &quot;ça plane pour moi&quot;</groundspeak:name>
            <groundspeak:placed_by>triboulet</groundspeak:placed_by>
            <groundspeak:owner id="1535">triboulet</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
