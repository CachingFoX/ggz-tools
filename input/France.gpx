        <url>http://opencachetest.garmin.com/#geocache/OX2P486</url>
        <urlname>Ligne Maginot des Alpes  &quot;Le fort Saint-Gobain&quot;</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2506542">
            <groundspeak:name>Ligne Maginot des Alpes  &quot;Le fort Saint-Gobain&quot;</groundspeak:name>
            <groundspeak:placed_by>triboulet</groundspeak:placed_by>
            <groundspeak:owner id="1535">triboulet</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
