        <groundspeak:cache archived="False" available="True" id="2129388">
            <groundspeak:name>le site Lamartine: &quot;Le lac&quot;</groundspeak:name>
            <groundspeak:placed_by>triboulet</groundspeak:placed_by>
            <groundspeak:owner id="1535">triboulet</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
