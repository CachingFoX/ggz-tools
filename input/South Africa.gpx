        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1597938">
            <groundspeak:name>Where the river ends.</groundspeak:name>
            <groundspeak:placed_by>gerhardoosMPSa</groundspeak:placed_by>
