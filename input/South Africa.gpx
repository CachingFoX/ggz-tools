        <groundspeak:cache archived="False" available="True" id="1308098">
            <groundspeak:name>Tonteldoos XI Machadodorp - Gold, Battles, Kruger</groundspeak:name>
            <groundspeak:placed_by>gerhardoosMPSa</groundspeak:placed_by>
            <groundspeak:owner id="4519">gerhardoosMPSa</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
