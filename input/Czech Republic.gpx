        <groundspeak:cache archived="False" available="True" id="905430">
            <groundspeak:name>ZOO Iris</groundspeak:name>
            <groundspeak:placed_by>MichalSaFlower</groundspeak:placed_by>
            <groundspeak:owner id="3401">MichalSaFlower</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
