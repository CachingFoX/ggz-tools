        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2468133">
            <groundspeak:name>Paço - Rio Âncora</groundspeak:name>
            <groundspeak:placed_by>jotapesantos</groundspeak:placed_by>
