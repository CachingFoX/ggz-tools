        <groundspeak:cache archived="False" available="True" id="1508322">
            <groundspeak:name>Museu Judaico</groundspeak:name>
            <groundspeak:placed_by>NMCOUTO</groundspeak:placed_by>
            <groundspeak:owner id="57966">NMCOUTO</groundspeak:owner>
