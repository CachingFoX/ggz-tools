        <groundspeak:cache archived="False" available="True" id="2935607">
            <groundspeak:name>Aviocar 6518</groundspeak:name>
            <groundspeak:placed_by>Hugo e Fabiana</groundspeak:placed_by>
            <groundspeak:owner id="73036">Hugo e Fabiana</groundspeak:owner>
