        <groundspeak:cache archived="False" available="True" id="3158086">
            <groundspeak:name>Escalhão Religiosa</groundspeak:name>
            <groundspeak:placed_by>Spellcaster</groundspeak:placed_by>
            <groundspeak:owner id="75680">Spellcaster</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
