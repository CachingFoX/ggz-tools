        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="1228925">
            <groundspeak:name>NG7 - Cruz das Mós</groundspeak:name>
            <groundspeak:placed_by>NMSPT</groundspeak:placed_by>
