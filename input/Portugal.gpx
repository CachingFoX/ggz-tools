        <groundspeak:cache archived="False" available="True" id="3284057">
            <groundspeak:name>Kingdom of the Ents</groundspeak:name>
            <groundspeak:placed_by>luisftas</groundspeak:placed_by>
            <groundspeak:owner id="87582">luisftas</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
