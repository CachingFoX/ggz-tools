        <groundspeak:cache archived="False" available="True" id="2467415">
            <groundspeak:name>Church Micro 1569 - Tendring</groundspeak:name>
            <groundspeak:placed_by>howzat</groundspeak:placed_by>
            <groundspeak:owner id="61774">howzat</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
