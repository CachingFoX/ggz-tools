        <groundspeak:cache archived="False" available="True" id="2008493">
            <groundspeak:name>Hovercraft gets lots of column inches</groundspeak:name>
            <groundspeak:placed_by>Uncle Streaky</groundspeak:placed_by>
            <groundspeak:owner id="13805">Uncle Streaky</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
