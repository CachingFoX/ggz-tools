        <name>OX2CYNA</name>
        <desc>1.-MILITIA EMBALSE PUENTE NUEVO by arpia, Traditional Cache(2.8/2.6/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2CYNA</url>
        <urlname>1.-MILITIA EMBALSE PUENTE NUEVO</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2233064">
