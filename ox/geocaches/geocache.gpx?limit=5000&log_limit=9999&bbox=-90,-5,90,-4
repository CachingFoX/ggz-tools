        <urlname>The Elf ,The Bard &amp; The Virgin</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2503584">
            <groundspeak:name>The Elf ,The Bard &amp; The Virgin</groundspeak:name>
            <groundspeak:placed_by>CHAMBERLAIN.</groundspeak:placed_by>
            <groundspeak:owner id="11683">CHAMBERLAIN.</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
