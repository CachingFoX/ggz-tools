        <name>OX134KN</name>
        <desc>Country House- Small Cache by JESTER1970, Traditional Cache(2.5/4.4/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX134KN</url>
        <urlname>Country House- Small Cache</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1017348">
            <groundspeak:name>Country House- Small Cache</groundspeak:name>
            <groundspeak:placed_by>JESTER1970</groundspeak:placed_by>
            <groundspeak:owner id="60075">JESTER1970</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
