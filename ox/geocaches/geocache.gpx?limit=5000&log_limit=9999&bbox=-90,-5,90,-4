        <desc>Ruta Circular Camino del Agua - Dípteros by SEBRING25, Traditional Cache(1.5/2.6/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTEBX</url>
        <urlname>Ruta Circular Camino del Agua - Dípteros</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="28464228">
            <groundspeak:name>Ruta Circular Camino del Agua - Dípteros</groundspeak:name>
            <groundspeak:placed_by>SEBRING25</groundspeak:placed_by>
