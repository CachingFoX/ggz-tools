            <groundspeak:name>Pocopán</groundspeak:name>
            <groundspeak:placed_by>io-sys</groundspeak:placed_by>
            <groundspeak:owner id="72588">io-sys</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
