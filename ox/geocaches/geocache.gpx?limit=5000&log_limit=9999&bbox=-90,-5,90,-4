        <groundspeak:cache archived="False" available="True" id="755330">
            <groundspeak:name>The Romans were here</groundspeak:name>
            <groundspeak:placed_by>lock1uk</groundspeak:placed_by>
            <groundspeak:owner id="60251">lock1uk</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
