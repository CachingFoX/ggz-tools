        <url>http://www.opencaching.com/#geocache/OX2Q5E1</url>
        <urlname>Poets &amp; Pilots</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2537475">
            <groundspeak:name>Poets &amp; Pilots</groundspeak:name>
            <groundspeak:placed_by>CHAMBERLAIN.</groundspeak:placed_by>
            <groundspeak:owner id="11683">CHAMBERLAIN.</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
