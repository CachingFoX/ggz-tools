        <groundspeak:cache archived="False" available="True" id="1998021">
            <groundspeak:name>History On 12th Line</groundspeak:name>
            <groundspeak:placed_by>geotrailguy</groundspeak:placed_by>
            <groundspeak:owner id="4707">geotrailguy</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
