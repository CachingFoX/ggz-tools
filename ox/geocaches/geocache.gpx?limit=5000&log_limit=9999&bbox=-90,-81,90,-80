        <groundspeak:cache archived="False" available="True" id="2111065">
            <groundspeak:name>An Everyday Application of High School Physics</groundspeak:name>
            <groundspeak:placed_by>Pasha88</groundspeak:placed_by>
            <groundspeak:owner id="49764">Pasha88</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
