        <name>OX2WVHR</name>
        <desc>Dying for Snow - A Whodunit Mystery Cache by Pasha88, Unknown Cache(2.5/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2WVHR</url>
        <urlname>Dying for Snow - A Whodunit Mystery Cache</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="2676936">
