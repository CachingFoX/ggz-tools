            <groundspeak:name>Second Onondaga Baptist Church - Caledonia</groundspeak:name>
            <groundspeak:placed_by>Pasha88</groundspeak:placed_by>
            <groundspeak:owner id="49764">Pasha88</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
