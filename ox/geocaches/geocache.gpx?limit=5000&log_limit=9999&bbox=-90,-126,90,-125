        <groundspeak:cache archived="False" available="True" id="2229302">
            <groundspeak:name>Vancouver Island Monkey Puzzle.</groundspeak:name>
            <groundspeak:placed_by>The_Pollywog</groundspeak:placed_by>
            <groundspeak:owner id="51945">The_Pollywog</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
            <groundspeak:container>Large</groundspeak:container>
            <groundspeak:difficulty>4</groundspeak:difficulty>
