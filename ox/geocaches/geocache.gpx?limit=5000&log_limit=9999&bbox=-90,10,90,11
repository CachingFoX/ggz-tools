        <name>OX3320Q</name>
        <desc>Ein Wahrzeichen der Backsteinmoderne by datenhamster, Multi-cache(2.0/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX3320Q</url>
        <urlname>Ein Wahrzeichen der Backsteinmoderne</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="2861881">
            <groundspeak:name>Ein Wahrzeichen der Backsteinmoderne</groundspeak:name>
            <groundspeak:placed_by>datenhamster</groundspeak:placed_by>
            <groundspeak:owner id="2156">datenhamster</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
            <groundspeak:difficulty>2</groundspeak:difficulty>
