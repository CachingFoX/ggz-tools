        <groundspeak:cache archived="False" available="True" id="2988521">
            <groundspeak:name>Schmiedeberg - Schöne Aussicht?!</groundspeak:name>
            <groundspeak:placed_by>TEUFELSENGEL</groundspeak:placed_by>
            <groundspeak:owner id="4142">TEUFELSENGEL</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
