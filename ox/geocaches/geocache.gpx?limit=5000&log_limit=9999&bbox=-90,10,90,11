            <groundspeak:name>Willkommen in Wernigerode - III</groundspeak:name>
            <groundspeak:placed_by>TEUFELSENGEL</groundspeak:placed_by>
            <groundspeak:owner id="4142">TEUFELSENGEL</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
