        <url>http://www.opencaching.com/#geocache/OXC9A71</url>
        <urlname>Warttürmchen</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="11360199">
            <groundspeak:name>Warttürmchen</groundspeak:name>
            <groundspeak:placed_by>Trollmond</groundspeak:placed_by>
            <groundspeak:owner id="4813">Trollmond</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
