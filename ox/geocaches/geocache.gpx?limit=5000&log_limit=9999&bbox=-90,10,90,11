        <name>OX379VA</name>
        <desc>Wernigeröder Aussichten 3 by TEUFELSENGEL, Unknown Cache(3.0/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX379VA</url>
        <urlname>Wernigeröder Aussichten 3</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="2988565">
            <groundspeak:name>Wernigeröder Aussichten 3</groundspeak:name>
            <groundspeak:placed_by>TEUFELSENGEL</groundspeak:placed_by>
            <groundspeak:owner id="4142">TEUFELSENGEL</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
