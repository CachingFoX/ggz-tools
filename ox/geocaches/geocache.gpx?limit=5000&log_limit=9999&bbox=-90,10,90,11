        <name>OX179QK</name>
        <desc>Irgendwo anders im Nirgendwo by B.F.K.C., Unknown Cache(3.0/1.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX179QK</url>
        <urlname>Irgendwo anders im Nirgendwo</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="1141439">
