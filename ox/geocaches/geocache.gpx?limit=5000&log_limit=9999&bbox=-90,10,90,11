        <desc>Blick in die Geschichte by JOACHIM.ESSLINGER, Traditional Cache(2.5/2.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTR4V</url>
        <urlname>Blick in die Geschichte</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="28473619">
            <groundspeak:name>Blick in die Geschichte</groundspeak:name>
            <groundspeak:placed_by>JOACHIM.ESSLINGER</groundspeak:placed_by>
            <groundspeak:owner id="53529">JOACHIM.ESSLINGER</groundspeak:owner>
