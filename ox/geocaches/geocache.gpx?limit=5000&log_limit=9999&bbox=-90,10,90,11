        <name>OXCBE80</name>
        <desc>In 80 Minuten um die Welt VI by Dr.Cool, Unknown Cache(2.0/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXCBE80</url>
        <urlname>In 80 Minuten um die Welt VI</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="11423655">
            <groundspeak:name>In 80 Minuten um die Welt VI</groundspeak:name>
            <groundspeak:placed_by>Dr.Cool</groundspeak:placed_by>
            <groundspeak:owner id="10996">Dr.Cool</groundspeak:owner>
