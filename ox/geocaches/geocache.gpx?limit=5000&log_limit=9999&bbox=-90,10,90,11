        <desc>Die Paten by TEUFELSENGEL, Unknown Cache(1.5/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX31BTA</url>
        <urlname>Die Paten</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="2811710">
            <groundspeak:name>Die Paten</groundspeak:name>
            <groundspeak:placed_by>TEUFELSENGEL</groundspeak:placed_by>
            <groundspeak:owner id="4142">TEUFELSENGEL</groundspeak:owner>
