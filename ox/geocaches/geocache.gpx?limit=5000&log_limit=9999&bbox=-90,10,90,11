        <groundspeak:cache archived="False" available="True" id="11968802">
            <groundspeak:name>Gotteshäuser: Kirche Bergstedt</groundspeak:name>
            <groundspeak:placed_by>geo_ndr</groundspeak:placed_by>
            <groundspeak:owner id="1002">geo_ndr</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
