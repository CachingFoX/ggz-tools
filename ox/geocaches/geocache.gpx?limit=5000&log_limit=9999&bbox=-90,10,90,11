            <groundspeak:name>Kampfeinsatz im Minenfeld</groundspeak:name>
            <groundspeak:placed_by>Dr.Cool</groundspeak:placed_by>
            <groundspeak:owner id="10996">Dr.Cool</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
