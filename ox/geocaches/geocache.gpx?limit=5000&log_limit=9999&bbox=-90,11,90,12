        <name>OXC8A9C</name>
        <desc>Geitnerskoppe by Trollmond, Multi-cache(1.8/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXC8A9C</url>
        <urlname>Geitnerskoppe</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="11330481">
            <groundspeak:name>Geitnerskoppe</groundspeak:name>
            <groundspeak:placed_by>Trollmond</groundspeak:placed_by>
            <groundspeak:owner id="4813">Trollmond</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
