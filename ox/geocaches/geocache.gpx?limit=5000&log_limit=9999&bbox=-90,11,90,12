            <groundspeak:name>Fjalars Schatz</groundspeak:name>
            <groundspeak:placed_by>derDoc</groundspeak:placed_by>
            <groundspeak:owner id="10146">derDoc</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
