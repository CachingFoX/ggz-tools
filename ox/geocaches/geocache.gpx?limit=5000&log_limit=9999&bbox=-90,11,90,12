        <desc>Marlinger Höhenweg by Lujason, Traditional Cache(2.0/2.5/2.8)</desc>
        <url>http://www.opencaching.com/#geocache/OX1W8AH</url>
        <urlname>Marlinger Höhenweg</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1735893">
            <groundspeak:name>Marlinger Höhenweg</groundspeak:name>
            <groundspeak:placed_by>Lujason</groundspeak:placed_by>
            <groundspeak:owner id="4849">Lujason</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
