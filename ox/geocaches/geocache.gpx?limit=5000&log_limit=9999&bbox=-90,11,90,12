        <groundspeak:cache archived="False" available="True" id="1899867">
            <groundspeak:name>Landesverwaltungsamt Magdeburg</groundspeak:name>
            <groundspeak:placed_by>evident</groundspeak:placed_by>
            <groundspeak:owner id="439">evident</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
