            <groundspeak:name>Pontives</groundspeak:name>
            <groundspeak:placed_by>Edgar44</groundspeak:placed_by>
            <groundspeak:owner id="56422">Edgar44</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
