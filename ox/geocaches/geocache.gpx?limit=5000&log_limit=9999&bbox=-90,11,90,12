        <groundspeak:cache archived="False" available="True" id="11417717">
            <groundspeak:name>Am Bremer Teich</groundspeak:name>
            <groundspeak:placed_by>magesi</groundspeak:placed_by>
            <groundspeak:owner id="19841">magesi</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
