        <groundspeak:cache archived="False" available="True" id="2040573">
            <groundspeak:name>Staatstheater Nürnberg</groundspeak:name>
            <groundspeak:placed_by>FLATHY</groundspeak:placed_by>
            <groundspeak:owner id="3636">FLATHY</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
