        <name>OX1CCFT</name>
        <desc>Schrotetal by Mr_Power, Traditional Cache(4.0/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1CCFT</url>
        <urlname>Schrotetal</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1293035">
            <groundspeak:name>Schrotetal</groundspeak:name>
            <groundspeak:placed_by>Mr_Power</groundspeak:placed_by>
            <groundspeak:owner id="3783">Mr_Power</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
