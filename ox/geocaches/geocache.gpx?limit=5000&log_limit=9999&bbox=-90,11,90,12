        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="1433188">
            <groundspeak:name>CLAUSTROPHOBIA</groundspeak:name>
            <groundspeak:placed_by>Kingfisherteam</groundspeak:placed_by>
            <groundspeak:owner id="5688">Kingfisherteam</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
