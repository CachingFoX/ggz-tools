    <wpt lon="11.243" lat="50.30885">
        <time>2011-12-09T08:00:00.000Z</time>
        <name>OX38QVW</name>
        <desc>History Son - Zur Wache by -KTA-, Traditional Cache(1.5/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX38QVW</url>
        <urlname>History Son - Zur Wache</urlname>
        <sym>Geocache</sym>
