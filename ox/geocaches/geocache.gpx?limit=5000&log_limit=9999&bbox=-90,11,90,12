            <groundspeak:name>[iz3jio] Monte Tomba</groundspeak:name>
            <groundspeak:placed_by>iz3jio</groundspeak:placed_by>
            <groundspeak:owner id="129007">iz3jio</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
