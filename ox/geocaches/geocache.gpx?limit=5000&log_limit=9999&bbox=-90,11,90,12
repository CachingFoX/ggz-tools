        <desc>Hand aufs Herz by Sabbi, Unknown Cache(4.5/2.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1Y4EP</url>
        <urlname>Hand aufs Herz</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="1791760">
            <groundspeak:name>Hand aufs Herz</groundspeak:name>
            <groundspeak:placed_by>Sabbi</groundspeak:placed_by>
