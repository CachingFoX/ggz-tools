        <url>http://www.opencaching.com/#geocache/OXCC616</url>
        <urlname>walk in the sand BONUS</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="11445547">
            <groundspeak:name>walk in the sand BONUS</groundspeak:name>
            <groundspeak:placed_by>grfld007</groundspeak:placed_by>
            <groundspeak:owner id="18811">grfld007</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
            <groundspeak:difficulty>2</groundspeak:difficulty>
