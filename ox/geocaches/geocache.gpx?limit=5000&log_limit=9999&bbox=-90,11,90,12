        <desc>Timmlalm by Kingfisherteam, Traditional Cache(2.0/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1EG9M</url>
        <urlname>Timmlalm</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1356270">
