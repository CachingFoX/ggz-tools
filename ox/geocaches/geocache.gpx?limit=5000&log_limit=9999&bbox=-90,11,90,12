        <time>2007-06-17T03:00:00.000-05:00</time>
        <name>OX13PAB</name>
        <desc>Historic Lost Place  by Kingfisherteam, Multi-cache(1.5/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX13PAB</url>
        <urlname>Historic Lost Place </urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="1034357">
            <groundspeak:name>Historic Lost Place </groundspeak:name>
            <groundspeak:placed_by>Kingfisherteam</groundspeak:placed_by>
