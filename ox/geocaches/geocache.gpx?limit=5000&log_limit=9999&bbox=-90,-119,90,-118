                <ox:difficulty>1.5</ox:difficulty>
                <ox:size>2.0</ox:size>
                <ox:terrain>2.5</ox:terrain>
            </ox:ratings>
        </ox:opencaching>
    </wpt>
    <wpt lon="-118.993383" lat="38.9737">
        <time>2012-09-20T08:00:00.000Z</time>
