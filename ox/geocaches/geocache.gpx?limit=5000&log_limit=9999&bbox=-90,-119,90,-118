        <desc>The Beverly Hills Zoo by CARLOSGUERRERA, Traditional Cache(1.0/1.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2RQVH</url>
        <urlname>The Beverly Hills Zoo</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2584952">
            <groundspeak:name>The Beverly Hills Zoo</groundspeak:name>
            <groundspeak:placed_by>CARLOSGUERRERA</groundspeak:placed_by>
            <groundspeak:owner id="19341">CARLOSGUERRERA</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
            <groundspeak:difficulty>1</groundspeak:difficulty>
