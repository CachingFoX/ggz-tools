        <groundspeak:cache archived="False" available="True" id="876123">
            <groundspeak:name>Witz Chili Won?</groundspeak:name>
            <groundspeak:placed_by>WITZABOUT</groundspeak:placed_by>
            <groundspeak:owner id="2891">WITZABOUT</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
