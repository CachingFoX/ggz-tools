        <url>http://www.opencaching.com/#geocache/OX2RKK0</url>
        <urlname>Swingbridge 209.1</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2580874">
            <groundspeak:name>Swingbridge 209.1</groundspeak:name>
            <groundspeak:placed_by>Blue Canary</groundspeak:placed_by>
            <groundspeak:owner id="17321">Blue Canary</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
