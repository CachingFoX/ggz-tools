        <desc>Bikernieki Memorial by PRUSUX, Virtual Cache(1.5/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTR2Q</url>
        <urlname>Bikernieki Memorial</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Virtual Cache</type>
        <groundspeak:cache archived="False" available="True" id="28473554">
            <groundspeak:name>Bikernieki Memorial</groundspeak:name>
            <groundspeak:placed_by>PRUSUX</groundspeak:placed_by>
            <groundspeak:owner id="3533">PRUSUX</groundspeak:owner>
            <groundspeak:type>Virtual Cache</groundspeak:type>
