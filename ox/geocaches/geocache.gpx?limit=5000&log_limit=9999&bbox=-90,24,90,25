        <groundspeak:cache archived="False" available="True" id="1734168">
            <groundspeak:name>Sliedes</groundspeak:name>
            <groundspeak:placed_by>normis</groundspeak:placed_by>
            <groundspeak:owner id="1151">normis</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
