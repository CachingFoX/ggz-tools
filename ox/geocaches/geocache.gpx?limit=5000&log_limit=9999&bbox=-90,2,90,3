        <name>OX39YY0</name>
        <desc>Ste Geneviève des Bois #2 : la Grotte by Dunnno, Traditional Cache(1.5/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX39YY0</url>
        <urlname>Ste Geneviève des Bois #2 : la Grotte</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="3067450">
            <groundspeak:name>Ste Geneviève des Bois #2 : la Grotte</groundspeak:name>
            <groundspeak:placed_by>Dunnno</groundspeak:placed_by>
            <groundspeak:owner id="55840">Dunnno</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
