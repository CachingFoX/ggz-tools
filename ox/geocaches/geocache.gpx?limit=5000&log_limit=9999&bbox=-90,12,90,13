        <groundspeak:cache archived="False" available="True" id="2737847">
            <groundspeak:name>Le Fontane Bianche</groundspeak:name>
            <groundspeak:placed_by>vatuttobene</groundspeak:placed_by>
            <groundspeak:owner id="78615">vatuttobene</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
