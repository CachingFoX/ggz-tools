            <groundspeak:name>Knauthainer Schloss</groundspeak:name>
            <groundspeak:placed_by>D1k74</groundspeak:placed_by>
            <groundspeak:owner id="3363">D1k74</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
