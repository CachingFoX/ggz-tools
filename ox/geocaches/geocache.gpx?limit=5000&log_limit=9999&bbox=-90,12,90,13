        <groundspeak:cache archived="False" available="True" id="1601702">
            <groundspeak:name>A93 - Exit 16</groundspeak:name>
            <groundspeak:placed_by>DALLHANS72</groundspeak:placed_by>
            <groundspeak:owner id="4068">DALLHANS72</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
