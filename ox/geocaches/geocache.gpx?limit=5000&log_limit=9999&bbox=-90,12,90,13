        <name>OXM0PY</name>
        <desc>Musik liegt in der Luft by hihatzz, Multi-cache(2.5/1.7/3.5)</desc>
        <url>http://www.opencaching.com/#geocache/OXM0PY</url>
        <urlname>Musik liegt in der Luft</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="596531">
            <groundspeak:name>Musik liegt in der Luft</groundspeak:name>
            <groundspeak:placed_by>hihatzz</groundspeak:placed_by>
            <groundspeak:owner id="3171">hihatzz</groundspeak:owner>
