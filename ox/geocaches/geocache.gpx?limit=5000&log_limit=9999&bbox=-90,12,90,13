        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="1950862">
            <groundspeak:name>Erinnerung</groundspeak:name>
            <groundspeak:placed_by>MARIANE_SCHNEIDER</groundspeak:placed_by>
            <groundspeak:owner id="627">MARIANE_SCHNEIDER</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
