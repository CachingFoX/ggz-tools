        <name>OX1NWE1</name>
        <desc>Fjelldalens Slott by MSEN, Traditional Cache(1.5/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1NWE1</url>
        <urlname>Fjelldalens Slott</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1575514">
            <groundspeak:name>Fjelldalens Slott</groundspeak:name>
            <groundspeak:placed_by>MSEN</groundspeak:placed_by>
            <groundspeak:owner id="4368">MSEN</groundspeak:owner>
