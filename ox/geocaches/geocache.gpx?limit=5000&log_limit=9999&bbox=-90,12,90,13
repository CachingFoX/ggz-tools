        <desc>Agent Orange II (Neu) - Sumpf ohne Wiederkehr by FLACO76, Unknown Cache(3.5/4.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX3G5VT</url>
        <urlname>Agent Orange II (Neu) - Sumpf ohne Wiederkehr</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="3252855">
            <groundspeak:name>Agent Orange II (Neu) - Sumpf ohne Wiederkehr</groundspeak:name>
            <groundspeak:placed_by>FLACO76</groundspeak:placed_by>
            <groundspeak:owner id="163351">FLACO76</groundspeak:owner>
