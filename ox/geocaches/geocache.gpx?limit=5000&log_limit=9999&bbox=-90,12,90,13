        <name>OX24JZ3</name>
        <desc>Naabwiese by dieboggns, Traditional Cache(3.5/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX24JZ3</url>
        <urlname>Naabwiese</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1984437">
            <groundspeak:name>Naabwiese</groundspeak:name>
            <groundspeak:placed_by>dieboggns</groundspeak:placed_by>
            <groundspeak:owner id="9370">dieboggns</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
