        <name>OX24H06</name>
        <desc>Gohliser Aktien Brauerei by riebecker, Traditional Cache(1.7/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX24H06</url>
        <urlname>Gohliser Aktien Brauerei</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1982549">
            <groundspeak:name>Gohliser Aktien Brauerei</groundspeak:name>
            <groundspeak:placed_by>riebecker</groundspeak:placed_by>
            <groundspeak:owner id="7753">riebecker</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
