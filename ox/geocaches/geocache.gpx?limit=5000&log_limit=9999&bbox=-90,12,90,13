        <desc>GOLIATH by FLACO76, Traditional Cache(4.5/5.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2X31D</url>
        <urlname>GOLIATH</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2684117">
            <groundspeak:name>GOLIATH</groundspeak:name>
            <groundspeak:placed_by>FLACO76</groundspeak:placed_by>
            <groundspeak:owner id="163351">FLACO76</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
