        <time>2010-12-09T08:41:59.000-06:00</time>
        <name>OX1K0E5</name>
        <desc>Die Lukaskirche in Leipzig-Volkmarsdorf by esther_and_family, Unknown Cache(2.5/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1K0E5</url>
        <urlname>Die Lukaskirche in Leipzig-Volkmarsdorf</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="1489989">
            <groundspeak:name>Die Lukaskirche in Leipzig-Volkmarsdorf</groundspeak:name>
            <groundspeak:placed_by>esther_and_family</groundspeak:placed_by>
            <groundspeak:owner id="3328">esther_and_family</groundspeak:owner>
