        <groundspeak:cache archived="False" available="True" id="1990576">
            <groundspeak:name>Bierdiplom: Sternquell Plauen II (Neuensalz)</groundspeak:name>
            <groundspeak:placed_by>listel</groundspeak:placed_by>
