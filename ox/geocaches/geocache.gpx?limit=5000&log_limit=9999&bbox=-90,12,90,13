        <groundspeak:cache archived="False" available="True" id="2238227">
            <groundspeak:name>Das Jedi-Rätsel</groundspeak:name>
            <groundspeak:placed_by>KLECKAGANDALF</groundspeak:placed_by>
            <groundspeak:owner id="361">KLECKAGANDALF</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
            <groundspeak:difficulty>4.5</groundspeak:difficulty>
