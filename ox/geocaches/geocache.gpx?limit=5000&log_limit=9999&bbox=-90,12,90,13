        <name>OX2Y2NR</name>
        <desc>Petersværft by mr.-oda, Traditional Cache(2.2/2.6/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2Y2NR</url>
        <urlname>Petersværft</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2713578">
            <groundspeak:name>Petersværft</groundspeak:name>
            <groundspeak:placed_by>mr.-oda</groundspeak:placed_by>
            <groundspeak:owner id="62120">mr.-oda</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
            <groundspeak:difficulty>2</groundspeak:difficulty>
