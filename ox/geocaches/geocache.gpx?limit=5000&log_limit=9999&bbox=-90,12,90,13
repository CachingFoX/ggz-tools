        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="2213046">
            <groundspeak:name>Geldwäsche in der Mafia-Molkerei [LP]</groundspeak:name>
            <groundspeak:placed_by>Geoborkis</groundspeak:placed_by>
            <groundspeak:owner id="1052">Geoborkis</groundspeak:owner>
