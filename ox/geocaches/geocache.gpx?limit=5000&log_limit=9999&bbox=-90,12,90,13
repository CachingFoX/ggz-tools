        <groundspeak:cache archived="False" available="True" id="1295925">
            <groundspeak:name>Apostelkirche</groundspeak:name>
            <groundspeak:placed_by>D1k74</groundspeak:placed_by>
            <groundspeak:owner id="3363">D1k74</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
