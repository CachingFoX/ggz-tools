        <time>2010-07-25T02:00:00.000-05:00</time>
        <name>OX2C8VJ</name>
        <desc>Geldwäsche in der Mafia-Molkerei [LP] by Geoborkis, Multi-cache(3.5/4.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2C8VJ</url>
        <urlname>Geldwäsche in der Mafia-Molkerei [LP]</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="2213046">
            <groundspeak:name>Geldwäsche in der Mafia-Molkerei [LP]</groundspeak:name>
            <groundspeak:placed_by>Geoborkis</groundspeak:placed_by>
            <groundspeak:owner id="1052">Geoborkis</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
            <groundspeak:difficulty>3.5</groundspeak:difficulty>
