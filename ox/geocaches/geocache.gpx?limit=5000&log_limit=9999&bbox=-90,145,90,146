            <groundspeak:name>Bushy Park</groundspeak:name>
            <groundspeak:placed_by>DATRUK</groundspeak:placed_by>
            <groundspeak:owner id="122312">DATRUK</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
