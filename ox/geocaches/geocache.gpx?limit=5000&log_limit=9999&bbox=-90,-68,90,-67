        <groundspeak:cache archived="False" available="True" id="1308516">
            <groundspeak:name>Porta Coeli, San Germán</groundspeak:name>
            <groundspeak:placed_by>MILTON_FMH</groundspeak:placed_by>
            <groundspeak:owner id="53251">MILTON_FMH</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
