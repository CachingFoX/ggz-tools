            </groundspeak:logs>
        </groundspeak:cache>
        <ox:opencaching>
            <ox:ratings>
                <ox:awesomeness>2.8</ox:awesomeness>
                <ox:difficulty>2.0</ox:difficulty>
                <ox:size>3.8</ox:size>
                <ox:terrain>1.9</ox:terrain>
            </ox:ratings>
