        <groundspeak:cache archived="False" available="True" id="3278993">
            <groundspeak:name>Albion Cottage / Hadfield Hurst / Hatfield Hall</groundspeak:name>
            <groundspeak:placed_by>ZACKBLACK</groundspeak:placed_by>
            <groundspeak:owner id="109945">ZACKBLACK</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
