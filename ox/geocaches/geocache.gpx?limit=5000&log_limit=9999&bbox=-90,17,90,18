            <groundspeak:name>Horar a pytliak 2</groundspeak:name>
            <groundspeak:placed_by>Rendezteam</groundspeak:placed_by>
            <groundspeak:owner id="8482">Rendezteam</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
