        <groundspeak:cache archived="False" available="True" id="1822327">
            <groundspeak:name>Hlusovicka piskovna / Hlusovice flooded sand-pit</groundspeak:name>
            <groundspeak:placed_by>BigT.cz</groundspeak:placed_by>
            <groundspeak:owner id="24285">BigT.cz</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
            <groundspeak:difficulty>3</groundspeak:difficulty>
