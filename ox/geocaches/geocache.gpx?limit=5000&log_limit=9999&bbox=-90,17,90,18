        <groundspeak:cache archived="False" available="True" id="1818532">
            <groundspeak:name>Trnavská Kostnica</groundspeak:name>
            <groundspeak:placed_by>Derick777</groundspeak:placed_by>
            <groundspeak:owner id="129812">Derick777</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
