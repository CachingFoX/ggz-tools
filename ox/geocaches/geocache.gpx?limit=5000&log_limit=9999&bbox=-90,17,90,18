        <desc>The Town of Queens (H) by BiharyG, Multi-cache(1.0/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXXQHH</url>
        <urlname>The Town of Queens (H)</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="856795">
            <groundspeak:name>The Town of Queens (H)</groundspeak:name>
            <groundspeak:placed_by>BiharyG</groundspeak:placed_by>
            <groundspeak:owner id="71494">BiharyG</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
            <groundspeak:difficulty>1</groundspeak:difficulty>
