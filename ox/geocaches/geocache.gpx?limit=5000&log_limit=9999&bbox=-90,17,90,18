        <groundspeak:cache archived="False" available="True" id="575475">
            <groundspeak:name>Cachticky hrad</groundspeak:name>
            <groundspeak:placed_by>J&amp;T&amp;R</groundspeak:placed_by>
            <groundspeak:owner id="5058">J&amp;T&amp;R</groundspeak:owner>
