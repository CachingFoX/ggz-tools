        <name>OX22B6V</name>
        <desc>Samorin - Synagoga // Samorin - Synagogue by Ceico a Martinka, Traditional Cache(1.3/1.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX22B6V</url>
        <urlname>Samorin - Synagoga // Samorin - Synagogue</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1917407">
            <groundspeak:name>Samorin - Synagoga // Samorin - Synagogue</groundspeak:name>
            <groundspeak:placed_by>Ceico a Martinka</groundspeak:placed_by>
            <groundspeak:owner id="22619">Ceico a Martinka</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
