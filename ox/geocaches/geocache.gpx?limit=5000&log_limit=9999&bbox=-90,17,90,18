        <name>OX101X7</name>
        <desc>Washing house by BiharyG, Traditional Cache(1.5/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX101X7</url>
        <urlname>Washing house</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="925357">
            <groundspeak:name>Washing house</groundspeak:name>
            <groundspeak:placed_by>BiharyG</groundspeak:placed_by>
            <groundspeak:owner id="71494">BiharyG</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
