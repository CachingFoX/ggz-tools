        <desc>Hamlet of Bonnefoi by gerhardoosMPSa, Traditional Cache(2.5/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX18PQC</url>
        <urlname>Hamlet of Bonnefoi</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1183716">
            <groundspeak:name>Hamlet of Bonnefoi</groundspeak:name>
            <groundspeak:placed_by>gerhardoosMPSa</groundspeak:placed_by>
