        <desc>Double Arched Venetian Bridge by Alekos_CY, Traditional Cache(1.5/1.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1EEGZ</url>
        <urlname>Double Arched Venetian Bridge</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1354575">
