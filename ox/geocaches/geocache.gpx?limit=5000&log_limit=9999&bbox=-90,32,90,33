        <name>OX1DEJR</name>
        <desc>Amiandos Destruction by Alekos_CY, Traditional Cache(2.0/3.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1DEJR</url>
        <urlname>Amiandos Destruction</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1324840">
            <groundspeak:name>Amiandos Destruction</groundspeak:name>
            <groundspeak:placed_by>Alekos_CY</groundspeak:placed_by>
            <groundspeak:owner id="147215">Alekos_CY</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
