            <groundspeak:name>Evil milk</groundspeak:name>
            <groundspeak:placed_by>mcano</groundspeak:placed_by>
            <groundspeak:owner id="3756">mcano</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
