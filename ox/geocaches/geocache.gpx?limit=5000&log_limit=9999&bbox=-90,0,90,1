        <name>OX201QW</name>
        <desc>Church Micro 910. Row Chapel by tigerthumbs, Traditional Cache(1.5/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX201QW</url>
        <urlname>Church Micro 910. Row Chapel</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1848743">
            <groundspeak:name>Church Micro 910. Row Chapel</groundspeak:name>
            <groundspeak:placed_by>tigerthumbs</groundspeak:placed_by>
            <groundspeak:owner id="58446">tigerthumbs</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
