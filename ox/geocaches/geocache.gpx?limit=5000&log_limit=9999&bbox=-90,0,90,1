        <groundspeak:cache archived="False" available="True" id="357960">
            <groundspeak:name>Roman Around</groundspeak:name>
            <groundspeak:placed_by>sandvika</groundspeak:placed_by>
            <groundspeak:owner id="5539">sandvika</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Large</groundspeak:container>
