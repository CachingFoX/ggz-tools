        <desc>National Trust Wicken Fen 2 by National Trust -  Wicken Fen, Traditional Cache(2.6/2.4/2.8)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTW2F</url>
        <urlname>National Trust Wicken Fen 2</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="28476429">
            <groundspeak:name>National Trust Wicken Fen 2</groundspeak:name>
            <groundspeak:placed_by>National Trust -  Wicken Fen</groundspeak:placed_by>
            <groundspeak:owner id="16389">National Trust -  Wicken Fen</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
            <groundspeak:difficulty>2.5</groundspeak:difficulty>
