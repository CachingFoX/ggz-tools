            <groundspeak:name>&quot;Wissensrunde&quot;</groundspeak:name>
            <groundspeak:placed_by>POLXS</groundspeak:placed_by>
            <groundspeak:owner id="340">POLXS</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
