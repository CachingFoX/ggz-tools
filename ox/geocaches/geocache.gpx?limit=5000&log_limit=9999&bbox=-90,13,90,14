        <sym>Geocache</sym>
        <type>Geocache|Virtual Cache</type>
        <groundspeak:cache archived="False" available="True" id="11482068">
            <groundspeak:name>Kurze Rast mit tollem Ausblick (rollifreundlich)</groundspeak:name>
            <groundspeak:placed_by>SecuRitty</groundspeak:placed_by>
            <groundspeak:owner id="133360">SecuRitty</groundspeak:owner>
            <groundspeak:type>Virtual Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
