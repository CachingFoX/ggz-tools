            <groundspeak:name>the smallest tram in the world</groundspeak:name>
            <groundspeak:placed_by>cyberswan</groundspeak:placed_by>
            <groundspeak:owner id="8092">cyberswan</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
