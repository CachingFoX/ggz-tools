        <urlname>Don Quijote de la Mancha</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2828994">
            <groundspeak:name>Don Quijote de la Mancha</groundspeak:name>
            <groundspeak:placed_by>Dragel</groundspeak:placed_by>
