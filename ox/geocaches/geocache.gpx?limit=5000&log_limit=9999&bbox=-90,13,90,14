        <groundspeak:cache archived="False" available="True" id="1729042">
            <groundspeak:name>Gotteshäuser: Kirche zum heiligen Erasmus </groundspeak:name>
            <groundspeak:placed_by>dr.dingens</groundspeak:placed_by>
            <groundspeak:owner id="67805">dr.dingens</groundspeak:owner>
