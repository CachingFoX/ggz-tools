        <desc>Treffen sich zwei Künstler... by Wotiii, Traditional Cache(1.3/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX3Y445</url>
        <urlname>Treffen sich zwei Künstler...</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="3638475">
            <groundspeak:name>Treffen sich zwei Künstler...</groundspeak:name>
            <groundspeak:placed_by>Wotiii</groundspeak:placed_by>
            <groundspeak:owner id="151047">Wotiii</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
