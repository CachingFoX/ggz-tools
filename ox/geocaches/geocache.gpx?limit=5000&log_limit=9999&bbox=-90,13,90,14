        <url>http://www.opencaching.com/#geocache/OX1CHED</url>
        <urlname>Koncentracni tabor - Holysov</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1297797">
            <groundspeak:name>Koncentracni tabor - Holysov</groundspeak:name>
            <groundspeak:placed_by>geo3AM</groundspeak:placed_by>
