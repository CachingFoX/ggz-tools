        <groundspeak:cache archived="False" available="True" id="187471">
            <groundspeak:name>Hansa's Viertel</groundspeak:name>
            <groundspeak:placed_by>moenk</groundspeak:placed_by>
            <groundspeak:owner id="3269">moenk</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
            <groundspeak:difficulty>2</groundspeak:difficulty>
