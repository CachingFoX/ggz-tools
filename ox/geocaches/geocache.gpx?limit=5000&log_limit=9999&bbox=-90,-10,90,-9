        <name>OX214DF</name>
        <desc>Museu do Fuzileiro by RavenMaster&amp;Uindinha, Traditional Cache(2.5/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX214DF</url>
        <urlname>Museu do Fuzileiro</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1881095">
            <groundspeak:name>Museu do Fuzileiro</groundspeak:name>
            <groundspeak:placed_by>RavenMaster&amp;Uindinha</groundspeak:placed_by>
