        <name>OXZTQKM</name>
        <desc>Cabeço Da Vela by jxbat, Traditional Cache(2.0/1.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTQKM</url>
        <urlname>Cabeço Da Vela</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="28473117">
            <groundspeak:name>Cabeço Da Vela</groundspeak:name>
            <groundspeak:placed_by>jxbat</groundspeak:placed_by>
            <groundspeak:owner id="10262">jxbat</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
