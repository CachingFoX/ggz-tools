            <groundspeak:name>Moinho de Mira Sintra</groundspeak:name>
            <groundspeak:placed_by>TeAm_PeReIrA</groundspeak:placed_by>
            <groundspeak:owner id="8612">TeAm_PeReIrA</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
