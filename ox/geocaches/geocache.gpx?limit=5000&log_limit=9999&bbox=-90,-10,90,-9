        <groundspeak:cache archived="False" available="True" id="2366378">
            <groundspeak:name>Cartuxa</groundspeak:name>
            <groundspeak:placed_by>Kelux</groundspeak:placed_by>
            <groundspeak:owner id="5118">Kelux</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
