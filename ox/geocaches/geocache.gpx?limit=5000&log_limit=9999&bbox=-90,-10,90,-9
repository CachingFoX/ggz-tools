        <groundspeak:cache archived="False" available="True" id="2603571">
            <groundspeak:name>Knight of the Blue River [Arrábida]</groundspeak:name>
            <groundspeak:placed_by>SawCastro</groundspeak:placed_by>
            <groundspeak:owner id="2692">SawCastro</groundspeak:owner>
