        <groundspeak:cache archived="False" available="True" id="1601763">
            <groundspeak:name>Moinhos da Malveira</groundspeak:name>
            <groundspeak:placed_by>GEOCACHERZONE</groundspeak:placed_by>
            <groundspeak:owner id="834">GEOCACHERZONE</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
