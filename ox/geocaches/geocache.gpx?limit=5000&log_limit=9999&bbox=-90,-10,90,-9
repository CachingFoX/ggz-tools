        <name>OX365A1</name>
        <desc>Geoquintas - Quinta da Verdaleira by Pcalisto, Traditional Cache(1.5/2.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX365A1</url>
        <urlname>Geoquintas - Quinta da Verdaleira</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2954425">
            <groundspeak:name>Geoquintas - Quinta da Verdaleira</groundspeak:name>
            <groundspeak:placed_by>Pcalisto</groundspeak:placed_by>
