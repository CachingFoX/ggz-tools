        <groundspeak:cache archived="False" available="True" id="1561208">
            <groundspeak:name>A Caverna [Torres Vedras]</groundspeak:name>
            <groundspeak:placed_by>nunotvedras</groundspeak:placed_by>
            <groundspeak:owner id="1671">nunotvedras</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
