        <groundspeak:cache archived="False" available="True" id="2969126">
            <groundspeak:name>MST - Pragal Interface v2 - Projecto Geogare</groundspeak:name>
            <groundspeak:placed_by>ravenmaster</groundspeak:placed_by>
            <groundspeak:owner id="997">ravenmaster</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Large</groundspeak:container>
            <groundspeak:difficulty>2.5</groundspeak:difficulty>
