        <desc>Lapa da Rainha [Torres Vedras] by nunotvedras, Traditional Cache(2.5/4.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1NX5F</url>
        <urlname>Lapa da Rainha [Torres Vedras]</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1576210">
            <groundspeak:name>Lapa da Rainha [Torres Vedras]</groundspeak:name>
            <groundspeak:placed_by>nunotvedras</groundspeak:placed_by>
