        <groundspeak:cache archived="False" available="True" id="1444982">
            <groundspeak:name>Carnaxide Antigua</groundspeak:name>
            <groundspeak:placed_by>Kelux</groundspeak:placed_by>
            <groundspeak:owner id="5118">Kelux</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
            <groundspeak:difficulty>2.5</groundspeak:difficulty>
