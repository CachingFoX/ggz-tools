        <name>OX2K0TE</name>
        <desc>One Honoured for the Road by Kelux, Traditional Cache(1.5/1.5/2.9)</desc>
        <url>http://www.opencaching.com/#geocache/OX2K0TE</url>
        <urlname>One Honoured for the Road</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2413860">
            <groundspeak:name>One Honoured for the Road</groundspeak:name>
            <groundspeak:placed_by>Kelux</groundspeak:placed_by>
            <groundspeak:owner id="5118">Kelux</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
