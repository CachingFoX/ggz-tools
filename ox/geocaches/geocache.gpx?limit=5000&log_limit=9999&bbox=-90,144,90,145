        <groundspeak:cache archived="False" available="True" id="2910328">
            <groundspeak:name>Swansea MK2</groundspeak:name>
            <groundspeak:placed_by>vkc1sr</groundspeak:placed_by>
            <groundspeak:owner id="110408">vkc1sr</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
