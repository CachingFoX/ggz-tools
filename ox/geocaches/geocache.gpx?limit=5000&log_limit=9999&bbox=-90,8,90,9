        <name>OX25JBQ</name>
        <desc>Sebi-rockt - Remember - John Bonham by sebi-rockt, Traditional Cache(1.5/1.6/2.8)</desc>
        <url>http://www.opencaching.com/#geocache/OX25JBQ</url>
        <urlname>Sebi-rockt - Remember - John Bonham</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2013659">
            <groundspeak:name>Sebi-rockt - Remember - John Bonham</groundspeak:name>
            <groundspeak:placed_by>sebi-rockt</groundspeak:placed_by>
            <groundspeak:owner id="9941">sebi-rockt</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
