        <groundspeak:cache archived="False" available="True" id="1514125">
            <groundspeak:name>Rietberg: LGS West - Das Bibeldorf</groundspeak:name>
            <groundspeak:placed_by>Eibel</groundspeak:placed_by>
            <groundspeak:owner id="878">Eibel</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
