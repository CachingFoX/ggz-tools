        <groundspeak:cache archived="False" available="True" id="1669104">
            <groundspeak:name>Covo Loco # 1955</groundspeak:name>
            <groundspeak:placed_by>Biendliwisi</groundspeak:placed_by>
            <groundspeak:owner id="10926">Biendliwisi</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
