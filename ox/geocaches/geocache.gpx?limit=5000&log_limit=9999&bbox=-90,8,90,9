        <name>OX232QT</name>
        <desc>Heimat der Roten Teufel by Giochetto, Unknown Cache(2.0/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX232QT</url>
        <urlname>Heimat der Roten Teufel</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="1939075">
            <groundspeak:name>Heimat der Roten Teufel</groundspeak:name>
            <groundspeak:placed_by>Giochetto</groundspeak:placed_by>
            <groundspeak:owner id="6981">Giochetto</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
