        <groundspeak:cache archived="False" available="True" id="2626833">
            <groundspeak:name>Knochenfund am Lostplace</groundspeak:name>
            <groundspeak:placed_by>maxzapp</groundspeak:placed_by>
            <groundspeak:owner id="10701">maxzapp</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
