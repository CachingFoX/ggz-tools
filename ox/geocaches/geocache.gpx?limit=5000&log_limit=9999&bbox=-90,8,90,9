            <groundspeak:name>Jüdischer Friedhof – Frankfurt </groundspeak:name>
            <groundspeak:placed_by>Lupo512</groundspeak:placed_by>
            <groundspeak:owner id="1663">Lupo512</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
