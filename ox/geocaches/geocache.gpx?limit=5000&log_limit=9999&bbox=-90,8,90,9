        <name>OX2MQ51</name>
        <desc>La Madonna delle Pieve by nebbiolo, Traditional Cache(2.5/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2MQ51</url>
        <urlname>La Madonna delle Pieve</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2465121">
            <groundspeak:name>La Madonna delle Pieve</groundspeak:name>
            <groundspeak:placed_by>nebbiolo</groundspeak:placed_by>
