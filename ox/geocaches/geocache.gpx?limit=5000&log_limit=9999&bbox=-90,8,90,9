        <name>OX15RRH</name>
        <desc>Attenbergskuppe by MABACO, Unknown Cache(2.0/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX15RRH</url>
        <urlname>Attenbergskuppe</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="1096301">
            <groundspeak:name>Attenbergskuppe</groundspeak:name>
            <groundspeak:placed_by>MABACO</groundspeak:placed_by>
            <groundspeak:owner id="5317">MABACO</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
