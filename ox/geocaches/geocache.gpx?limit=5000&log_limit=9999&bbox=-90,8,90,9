        <name>OX1YPD7</name>
        <desc>Im Land der Trolle 2 by wolke07, Multi-cache(2.5/2.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1YPD7</url>
        <urlname>Im Land der Trolle 2</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="1809012">
            <groundspeak:name>Im Land der Trolle 2</groundspeak:name>
            <groundspeak:placed_by>wolke07</groundspeak:placed_by>
            <groundspeak:owner id="5347">wolke07</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
