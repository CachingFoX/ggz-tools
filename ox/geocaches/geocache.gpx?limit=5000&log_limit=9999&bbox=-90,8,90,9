        <urlname>Bombenalarm in den Tongruben</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1563068">
            <groundspeak:name>Bombenalarm in den Tongruben</groundspeak:name>
            <groundspeak:placed_by>staubfinger0702</groundspeak:placed_by>
            <groundspeak:owner id="4325">staubfinger0702</groundspeak:owner>
