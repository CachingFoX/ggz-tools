            <groundspeak:name>Traditional: Jack's 30. Birthday</groundspeak:name>
            <groundspeak:placed_by>Jack_Hunter</groundspeak:placed_by>
            <groundspeak:owner id="496">Jack_Hunter</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
