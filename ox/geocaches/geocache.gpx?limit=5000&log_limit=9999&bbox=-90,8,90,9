        <groundspeak:cache archived="False" available="True" id="876682">
            <groundspeak:name>Arkeburg</groundspeak:name>
            <groundspeak:placed_by>Lujason</groundspeak:placed_by>
            <groundspeak:owner id="4849">Lujason</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
