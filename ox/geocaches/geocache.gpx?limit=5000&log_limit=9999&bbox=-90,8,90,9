        <name>OXKEV4</name>
        <desc>Gaustatoppen by Reodor, Traditional Cache(2.4/3.4/2.9)</desc>
        <url>http://www.opencaching.com/#geocache/OXKEV4</url>
        <urlname>Gaustatoppen</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="580293">
            <groundspeak:name>Gaustatoppen</groundspeak:name>
            <groundspeak:placed_by>Reodor</groundspeak:placed_by>
            <groundspeak:owner id="10653">Reodor</groundspeak:owner>
