        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1060820">
            <groundspeak:name>Buffalo Bill in Varenesch</groundspeak:name>
            <groundspeak:placed_by>Lujason</groundspeak:placed_by>
            <groundspeak:owner id="4849">Lujason</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
