        <name>OX15FCH</name>
        <desc>Herrenwald by RIGICACHE, Traditional Cache(2.0/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX15FCH</url>
        <urlname>Herrenwald</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1087280">
            <groundspeak:name>Herrenwald</groundspeak:name>
            <groundspeak:placed_by>RIGICACHE</groundspeak:placed_by>
            <groundspeak:owner id="3202">RIGICACHE</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
