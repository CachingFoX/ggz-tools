        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1774367">
            <groundspeak:name>Rüsper Höhe</groundspeak:name>
            <groundspeak:placed_by>wittijack</groundspeak:placed_by>
