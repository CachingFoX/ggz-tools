            <groundspeak:name>Freizeitanlage Moos</groundspeak:name>
            <groundspeak:placed_by>atheris</groundspeak:placed_by>
            <groundspeak:owner id="19281">atheris</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
