            <groundspeak:name>Bell's Mystery</groundspeak:name>
            <groundspeak:placed_by>PWCINPC2001</groundspeak:placed_by>
            <groundspeak:owner id="7189">PWCINPC2001</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
