        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="1327718">
            <groundspeak:name>Cave Hill - Just Passing Through</groundspeak:name>
            <groundspeak:placed_by>AIRRAIDFAN</groundspeak:placed_by>
