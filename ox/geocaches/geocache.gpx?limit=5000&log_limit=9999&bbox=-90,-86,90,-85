        <urlname>Hall of Fame: SOLAR</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1355394">
            <groundspeak:name>Hall of Fame: SOLAR</groundspeak:name>
            <groundspeak:placed_by>Martin from Franklin GA</groundspeak:placed_by>
            <groundspeak:owner id="21991">Martin from Franklin GA</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
