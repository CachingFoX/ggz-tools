        <groundspeak:cache archived="False" available="True" id="28461348">
            <groundspeak:name>Orchard Knob: A Forgotten Piece of History</groundspeak:name>
            <groundspeak:placed_by>TNPHOTOBUG</groundspeak:placed_by>
            <groundspeak:owner id="165694">TNPHOTOBUG</groundspeak:owner>
