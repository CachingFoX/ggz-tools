            <groundspeak:name>Go Green</groundspeak:name>
            <groundspeak:placed_by>Honkeon</groundspeak:placed_by>
            <groundspeak:owner id="23638">Honkeon</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
