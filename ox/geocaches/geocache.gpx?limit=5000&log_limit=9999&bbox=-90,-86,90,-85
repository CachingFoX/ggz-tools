            <groundspeak:name>Nature Trail - Chirp</groundspeak:name>
            <groundspeak:placed_by>CLIMBGUY</groundspeak:placed_by>
            <groundspeak:owner id="6119">CLIMBGUY</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
