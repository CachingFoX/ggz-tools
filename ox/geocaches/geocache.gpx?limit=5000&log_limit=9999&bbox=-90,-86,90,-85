        <desc>Apalachicola Northern Railroad by Nashional, Traditional Cache(1.5/1.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXNT15</url>
        <urlname>Apalachicola Northern Railroad</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="649672">
            <groundspeak:name>Apalachicola Northern Railroad</groundspeak:name>
            <groundspeak:placed_by>Nashional</groundspeak:placed_by>
            <groundspeak:owner id="2838">Nashional</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
