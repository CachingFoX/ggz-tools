        <desc>Viecha by MIRO7777, Traditional Cache(2.8/4.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX33DFX</url>
        <urlname>Viecha</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2872922">
            <groundspeak:name>Viecha</groundspeak:name>
            <groundspeak:placed_by>MIRO7777</groundspeak:placed_by>
            <groundspeak:owner id="142521">MIRO7777</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
            <groundspeak:difficulty>3</groundspeak:difficulty>
