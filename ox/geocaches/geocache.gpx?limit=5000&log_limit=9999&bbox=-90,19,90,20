    <wpt lon="19.98205" lat="50.2047">
        <time>2012-03-30T00:00:00.000Z</time>
        <name>OXZTK83</name>
        <desc>Bar [OiM #5] by LzaEla, Unknown Cache(3.0/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTK83</url>
        <urlname>Bar [OiM #5]</urlname>
        <sym>Geocache</sym>
