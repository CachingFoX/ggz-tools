            <groundspeak:name>DC Dynamo (Wellington)</groundspeak:name>
            <groundspeak:placed_by>Emvy_Us</groundspeak:placed_by>
            <groundspeak:owner id="19984">Emvy_Us</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
