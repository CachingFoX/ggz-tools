        <name>OX2H117</name>
        <desc>Super Star | Étoile filante by ACGA, Traditional Cache(2.5/4.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2H117</url>
        <urlname>Super Star | Étoile filante</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2354488">
            <groundspeak:name>Super Star | Étoile filante</groundspeak:name>
            <groundspeak:placed_by>ACGA</groundspeak:placed_by>
            <groundspeak:owner id="600">ACGA</groundspeak:owner>
