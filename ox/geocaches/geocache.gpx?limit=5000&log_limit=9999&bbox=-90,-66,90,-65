        <name>OXZTQN0</name>
        <desc>Sheldon Cabin by ACGA, Multi-cache(3.2/4.2/3.2)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTQN0</url>
        <urlname>Sheldon Cabin</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="28473159">
            <groundspeak:name>Sheldon Cabin</groundspeak:name>
