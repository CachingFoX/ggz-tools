        <desc>Hug a Tree by ACGA, Traditional Cache(2.5/2.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2V7EB</url>
        <urlname>Hug a Tree</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2628780">
            <groundspeak:name>Hug a Tree</groundspeak:name>
            <groundspeak:placed_by>ACGA</groundspeak:placed_by>
            <groundspeak:owner id="600">ACGA</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
