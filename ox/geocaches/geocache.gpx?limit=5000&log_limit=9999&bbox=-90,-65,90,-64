        <url>http://www.opencaching.com/#geocache/OX1E6GT</url>
        <urlname>The Four Corners of Moncton #2 (Upper Coverdale)</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1346882">
            <groundspeak:name>The Four Corners of Moncton #2 (Upper Coverdale)</groundspeak:name>
            <groundspeak:placed_by>^Zor^</groundspeak:placed_by>
            <groundspeak:owner id="4675">^Zor^</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
