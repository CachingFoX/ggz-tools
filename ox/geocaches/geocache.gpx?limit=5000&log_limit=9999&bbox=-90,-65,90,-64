        <groundspeak:cache archived="False" available="True" id="3332107">
            <groundspeak:name>A &quot;Covered&quot; story</groundspeak:name>
            <groundspeak:placed_by>Cache Up NB</groundspeak:placed_by>
            <groundspeak:owner id="125342">Cache Up NB</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
