            <groundspeak:name>Acadian Forest - Forêt Acadienne </groundspeak:name>
            <groundspeak:placed_by>ACGA</groundspeak:placed_by>
            <groundspeak:owner id="600">ACGA</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
