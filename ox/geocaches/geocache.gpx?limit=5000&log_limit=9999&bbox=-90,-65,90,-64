        <name>OX1G0B7</name>
        <desc>Road Rut by ^Zor^, Traditional Cache(2.0/2.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1G0B7</url>
        <urlname>Road Rut</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1400525">
            <groundspeak:name>Road Rut</groundspeak:name>
            <groundspeak:placed_by>^Zor^</groundspeak:placed_by>
            <groundspeak:owner id="4675">^Zor^</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
            <groundspeak:difficulty>2</groundspeak:difficulty>
