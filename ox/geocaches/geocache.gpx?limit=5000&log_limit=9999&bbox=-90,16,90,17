        <name>OXMF86</name>
        <desc>HappyBox by Muckobanda, Traditional Cache(2.0/2.0/3.3)</desc>
        <url>http://www.opencaching.com/#geocache/OXMF86</url>
        <urlname>HappyBox</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="610489">
            <groundspeak:name>HappyBox</groundspeak:name>
            <groundspeak:placed_by>Muckobanda</groundspeak:placed_by>
            <groundspeak:owner id="4937">Muckobanda</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
