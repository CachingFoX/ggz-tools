        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1360348">
            <groundspeak:name>22_die tierkreiszeichen</groundspeak:name>
            <groundspeak:placed_by>schluchtenhaxler</groundspeak:placed_by>
            <groundspeak:owner id="8835">schluchtenhaxler</groundspeak:owner>
