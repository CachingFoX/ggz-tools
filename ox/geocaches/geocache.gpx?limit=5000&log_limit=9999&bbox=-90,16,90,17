        <groundspeak:cache archived="False" available="True" id="965192">
            <groundspeak:name>Römische Palastanlage</groundspeak:name>
            <groundspeak:placed_by>RoterOktoberDiver</groundspeak:placed_by>
            <groundspeak:owner id="1499">RoterOktoberDiver</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
