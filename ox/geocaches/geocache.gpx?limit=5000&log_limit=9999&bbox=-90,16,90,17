        <name>OX1ZJRM</name>
        <desc>CMNS - Wolfs by FREISMUTH56, Traditional Cache(1.5/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1ZJRM</url>
        <urlname>CMNS - Wolfs</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1835313">
            <groundspeak:name>CMNS - Wolfs</groundspeak:name>
            <groundspeak:placed_by>FREISMUTH56</groundspeak:placed_by>
            <groundspeak:owner id="10390">FREISMUTH56</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
