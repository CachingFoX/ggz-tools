        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="654333">
            <groundspeak:name>M&amp;M-37 Flatwood Floridays</groundspeak:name>
            <groundspeak:placed_by>M&amp;M_Melted</groundspeak:placed_by>
            <groundspeak:owner id="941">M&amp;M_Melted</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
