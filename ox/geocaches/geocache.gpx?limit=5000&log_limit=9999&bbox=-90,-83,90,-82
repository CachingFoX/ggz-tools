        <url>http://www.opencaching.com/#geocache/OX13A99</url>
        <urlname>Captain Kid's Haunted Pine Island Treasure</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1022792">
            <groundspeak:name>Captain Kid's Haunted Pine Island Treasure</groundspeak:name>
