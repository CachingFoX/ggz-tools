        <groundspeak:cache archived="False" available="True" id="573587">
            <groundspeak:name>Cougar, Tweetsie's Box and the End</groundspeak:name>
            <groundspeak:placed_by>KONNAROCK KID</groundspeak:placed_by>
            <groundspeak:owner id="5264">KONNAROCK KID</groundspeak:owner>
