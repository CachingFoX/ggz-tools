        <name>OX19GZV</name>
        <desc>Above The Blue Hole - Hoot Owl Rock by KONNAROCK KID, Traditional Cache(3.5/4.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX19GZV</url>
        <urlname>Above The Blue Hole - Hoot Owl Rock</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1207972">
            <groundspeak:name>Above The Blue Hole - Hoot Owl Rock</groundspeak:name>
