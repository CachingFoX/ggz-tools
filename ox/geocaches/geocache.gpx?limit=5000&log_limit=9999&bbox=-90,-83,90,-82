        <desc>The WAC and the Doughboy by KONNAROCK KID, Multi-cache(2.0/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXV7CF</url>
        <urlname>The WAC and the Doughboy</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="781680">
            <groundspeak:name>The WAC and the Doughboy</groundspeak:name>
            <groundspeak:placed_by>KONNAROCK KID</groundspeak:placed_by>
            <groundspeak:owner id="5264">KONNAROCK KID</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
            <groundspeak:difficulty>2</groundspeak:difficulty>
