                <groundspeak:log id="1147415">
                    <groundspeak:date>2010-05-18T14:00:00.000-05:00</groundspeak:date>
                    <groundspeak:type>Comment</groundspeak:type>
                    <groundspeak:finder id="12571">cache_n_out</groundspeak:finder>
                    <groundspeak:text encoded="False">Just glanced over at the &quot;attributes&quot; column.  We had everything on our trip, but the snake!</groundspeak:text>
