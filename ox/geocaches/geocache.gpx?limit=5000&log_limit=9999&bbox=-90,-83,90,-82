            <groundspeak:name>Cemetery Ridge Cache</groundspeak:name>
            <groundspeak:placed_by>SNBGEO07</groundspeak:placed_by>
            <groundspeak:owner id="13605">SNBGEO07</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
