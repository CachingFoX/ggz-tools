        <groundspeak:cache archived="False" available="True" id="989657">
            <groundspeak:name>M&amp;M-126 Taking the Plunge</groundspeak:name>
            <groundspeak:placed_by>M&amp;M_Melted</groundspeak:placed_by>
            <groundspeak:owner id="941">M&amp;M_Melted</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
