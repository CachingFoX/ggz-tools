        <groundspeak:cache archived="False" available="True" id="2773351">
            <groundspeak:name>Welcome to Brasov City DN73 - reloaded</groundspeak:name>
            <groundspeak:placed_by>Szilard</groundspeak:placed_by>
            <groundspeak:owner id="53681">Szilard</groundspeak:owner>
