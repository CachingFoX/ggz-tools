        <name>OXCBE25</name>
        <desc>MDG 4 by finep, Unknown Cache(2.0/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXCBE25</url>
        <urlname>MDG 4</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="11423474">
            <groundspeak:name>MDG 4</groundspeak:name>
            <groundspeak:placed_by>finep</groundspeak:placed_by>
            <groundspeak:owner id="17462">finep</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
