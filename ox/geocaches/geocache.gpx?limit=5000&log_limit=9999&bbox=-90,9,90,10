        <groundspeak:cache archived="False" available="True" id="1849317">
            <groundspeak:name>Der Damm</groundspeak:name>
            <groundspeak:placed_by>EVILROCCO</groundspeak:placed_by>
            <groundspeak:owner id="5718">EVILROCCO</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
