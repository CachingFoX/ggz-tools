        <groundspeak:cache archived="False" available="True" id="2423092">
            <groundspeak:name>Chirp Letterbox Munkbrarup</groundspeak:name>
            <groundspeak:placed_by>munki7</groundspeak:placed_by>
            <groundspeak:owner id="3428">munki7</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
            <groundspeak:difficulty>2</groundspeak:difficulty>
