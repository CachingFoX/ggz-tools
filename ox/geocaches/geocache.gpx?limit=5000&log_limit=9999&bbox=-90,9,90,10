        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2400017">
            <groundspeak:name>Wilseder Berghütte - Heide-Safari 8</groundspeak:name>
            <groundspeak:placed_by>s-e-b-b</groundspeak:placed_by>
            <groundspeak:owner id="48276">s-e-b-b</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
