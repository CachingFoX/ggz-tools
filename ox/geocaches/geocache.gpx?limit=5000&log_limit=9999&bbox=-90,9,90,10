        <groundspeak:cache archived="False" available="True" id="1243431">
            <groundspeak:name>Mühle Owschlag</groundspeak:name>
            <groundspeak:placed_by>CAPELLMEISTER</groundspeak:placed_by>
            <groundspeak:owner id="3567">CAPELLMEISTER</groundspeak:owner>
