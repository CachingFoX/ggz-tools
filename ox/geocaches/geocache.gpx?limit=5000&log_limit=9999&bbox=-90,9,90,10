        <groundspeak:cache archived="False" available="True" id="482822">
            <groundspeak:name>Der Eremit</groundspeak:name>
            <groundspeak:placed_by>ksmichel</groundspeak:placed_by>
            <groundspeak:owner id="5307">ksmichel</groundspeak:owner>
