        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="646675">
            <groundspeak:name>Enigma 2 - Die drei Walzen - Wooden Activity</groundspeak:name>
            <groundspeak:placed_by>KATZENOHR</groundspeak:placed_by>
            <groundspeak:owner id="5254">KATZENOHR</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
