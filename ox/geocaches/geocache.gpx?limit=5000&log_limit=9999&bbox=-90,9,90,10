        <urlname>Såta</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1842742">
            <groundspeak:name>Såta</groundspeak:name>
            <groundspeak:placed_by>Reodor</groundspeak:placed_by>
