        <name>OX2J2K9</name>
        <desc>Die FlaK by EVILROCCO, Multi-cache(2.5/5.0/3.5)</desc>
        <url>http://www.opencaching.com/#geocache/OX2J2K9</url>
        <urlname>Die FlaK</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="2385800">
            <groundspeak:name>Die FlaK</groundspeak:name>
            <groundspeak:placed_by>EVILROCCO</groundspeak:placed_by>
            <groundspeak:owner id="5718">EVILROCCO</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
