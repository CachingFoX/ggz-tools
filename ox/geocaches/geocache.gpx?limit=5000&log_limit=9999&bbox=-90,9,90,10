        <groundspeak:cache archived="False" available="True" id="1455036">
            <groundspeak:name>St. Michael</groundspeak:name>
            <groundspeak:placed_by>4free</groundspeak:placed_by>
            <groundspeak:owner id="1282">4free</groundspeak:owner>
