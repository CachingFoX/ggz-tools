        <groundspeak:cache archived="False" available="True" id="1744392">
            <groundspeak:name>Hans Holtorf</groundspeak:name>
            <groundspeak:placed_by>munki7</groundspeak:placed_by>
            <groundspeak:owner id="3428">munki7</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
