        <desc>Hünengrab by munki7, Traditional Cache(2.1/2.5/2.9)</desc>
        <url>http://www.opencaching.com/#geocache/OX1WH5J</url>
        <urlname>Hünengrab</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1744388">
            <groundspeak:name>Hünengrab</groundspeak:name>
            <groundspeak:placed_by>munki7</groundspeak:placed_by>
            <groundspeak:owner id="3428">munki7</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
