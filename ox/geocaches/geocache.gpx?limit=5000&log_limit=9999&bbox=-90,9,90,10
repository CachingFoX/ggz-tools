        <groundspeak:cache archived="False" available="True" id="2141240">
            <groundspeak:name>vom Bit zum Chip</groundspeak:name>
            <groundspeak:placed_by>ms-fk</groundspeak:placed_by>
            <groundspeak:owner id="412">ms-fk</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
