        <name>OX1HF1B</name>
        <desc>Haltet den Dieb! by ksmichel, Multi-cache(3.3/2.1/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1HF1B</url>
        <urlname>Haltet den Dieb!</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="1444425">
            <groundspeak:name>Haltet den Dieb!</groundspeak:name>
            <groundspeak:placed_by>ksmichel</groundspeak:placed_by>
            <groundspeak:owner id="5307">ksmichel</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
