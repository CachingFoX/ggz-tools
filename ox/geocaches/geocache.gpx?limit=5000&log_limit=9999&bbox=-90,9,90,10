        <groundspeak:cache archived="False" available="True" id="1302042">
            <groundspeak:name>Gotteshäuser: Stadtkirche Bad Arolsen</groundspeak:name>
            <groundspeak:placed_by>schalker01</groundspeak:placed_by>
            <groundspeak:owner id="4212">schalker01</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
