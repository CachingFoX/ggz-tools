        <name>OX2NJ1J</name>
        <desc>Vand på maskine by Erik FR, Traditional Cache(2.5/3.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2NJ1J</url>
        <urlname>Vand på maskine</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2490000">
            <groundspeak:name>Vand på maskine</groundspeak:name>
            <groundspeak:placed_by>Erik FR</groundspeak:placed_by>
            <groundspeak:owner id="38237">Erik FR</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
