            <groundspeak:name>Gegen das Vergessen</groundspeak:name>
            <groundspeak:placed_by>team_jcmg</groundspeak:placed_by>
            <groundspeak:owner id="2980">team_jcmg</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
