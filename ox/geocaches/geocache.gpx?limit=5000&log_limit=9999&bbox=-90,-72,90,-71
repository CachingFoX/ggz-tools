        <groundspeak:cache archived="False" available="True" id="1957992">
            <groundspeak:name>Gogol (nombre)</groundspeak:name>
            <groundspeak:placed_by>DANATISY</groundspeak:placed_by>
            <groundspeak:owner id="9982">DANATISY</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
