        <groundspeak:cache archived="False" available="True" id="640239">
            <groundspeak:name>Bacon, Fitch, Ashby, Clark Mill Site</groundspeak:name>
            <groundspeak:placed_by>bigqueue</groundspeak:placed_by>
            <groundspeak:owner id="86885">bigqueue</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
            <groundspeak:difficulty>2.5</groundspeak:difficulty>
