        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2298377">
            <groundspeak:name>State of Confusion</groundspeak:name>
            <groundspeak:placed_by>MVALENTINE6705</groundspeak:placed_by>
            <groundspeak:owner id="18125">MVALENTINE6705</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
