        <desc>Into The Labyrinth. by WillowFox, Traditional Cache(1.6/2.3/3.1)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTJ3N</url>
        <urlname>Into The Labyrinth.</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="28467817">
            <groundspeak:name>Into The Labyrinth.</groundspeak:name>
            <groundspeak:placed_by>WillowFox</groundspeak:placed_by>
            <groundspeak:owner id="115181">WillowFox</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
