        <name>OX2F4M6</name>
        <desc>State of Confusion by MVALENTINE6705, Traditional Cache(2.5/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2F4M6</url>
        <urlname>State of Confusion</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2298377">
            <groundspeak:name>State of Confusion</groundspeak:name>
            <groundspeak:placed_by>MVALENTINE6705</groundspeak:placed_by>
