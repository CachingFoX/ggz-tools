        <groundspeak:cache archived="False" available="True" id="1653430">
            <groundspeak:name>Black Belt</groundspeak:name>
            <groundspeak:placed_by>MAJIKLMOON</groundspeak:placed_by>
            <groundspeak:owner id="73873">MAJIKLMOON</groundspeak:owner>
