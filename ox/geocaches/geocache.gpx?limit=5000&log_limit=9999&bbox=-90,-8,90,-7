    <wpt lon="-7.8560333" lat="40.1533">
        <time>2009-09-12T07:00:00.000Z</time>
        <name>OX1YN8D</name>
        <desc>Açude do Ceiroco by 1bertoc, Traditional Cache(2.2/3.0/3.1)</desc>
        <url>http://www.opencaching.com/#geocache/OX1YN8D</url>
        <urlname>Açude do Ceiroco</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1807902">
            <groundspeak:name>Açude do Ceiroco</groundspeak:name>
            <groundspeak:placed_by>1bertoc</groundspeak:placed_by>
            <groundspeak:owner id="3063">1bertoc</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
