        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1807902">
            <groundspeak:name>Açude do Ceiroco</groundspeak:name>
            <groundspeak:placed_by>1bertoc</groundspeak:placed_by>
            <groundspeak:owner id="3063">1bertoc</groundspeak:owner>
