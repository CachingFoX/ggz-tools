        <time>2008-07-30T03:00:00.000-05:00</time>
        <name>OX1EKWZ</name>
        <desc>Parque Zoológico de Vila Flor by musa, Traditional Cache(1.5/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1EKWZ</url>
        <urlname>Parque Zoológico de Vila Flor</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1359721">
            <groundspeak:name>Parque Zoológico de Vila Flor</groundspeak:name>
            <groundspeak:placed_by>musa</groundspeak:placed_by>
            <groundspeak:owner id="6910">musa</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
