        <name>OX391Y8</name>
        <desc>Abbey Meadow, KCAT and Kings River by mr.-oda, Traditional Cache(2.0/1.6/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX391Y8</url>
        <urlname>Abbey Meadow, KCAT and Kings River</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="3040550">
            <groundspeak:name>Abbey Meadow, KCAT and Kings River</groundspeak:name>
            <groundspeak:placed_by>mr.-oda</groundspeak:placed_by>
            <groundspeak:owner id="62120">mr.-oda</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
