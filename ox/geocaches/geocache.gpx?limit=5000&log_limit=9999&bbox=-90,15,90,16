            <groundspeak:name>STP Skyline</groundspeak:name>
            <groundspeak:placed_by>f1nn</groundspeak:placed_by>
            <groundspeak:owner id="55598">f1nn</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
