        <desc>Mammut Baeume by Geo-Johnny, Traditional Cache(2.0/2.3/2.8)</desc>
        <url>http://www.opencaching.com/#geocache/OXY3W3</url>
        <urlname>Mammut Baeume</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="867662">
            <groundspeak:name>Mammut Baeume</groundspeak:name>
            <groundspeak:placed_by>Geo-Johnny</groundspeak:placed_by>
            <groundspeak:owner id="293">Geo-Johnny</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
