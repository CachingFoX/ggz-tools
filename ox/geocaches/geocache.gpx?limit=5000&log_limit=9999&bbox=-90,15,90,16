        <name>OX1T3HX</name>
        <desc>Platform Porschestraße by f1nn, Traditional Cache(1.6/1.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1T3HX</url>
        <urlname>Platform Porschestraße</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1671734">
            <groundspeak:name>Platform Porschestraße</groundspeak:name>
            <groundspeak:placed_by>f1nn</groundspeak:placed_by>
            <groundspeak:owner id="55598">f1nn</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
