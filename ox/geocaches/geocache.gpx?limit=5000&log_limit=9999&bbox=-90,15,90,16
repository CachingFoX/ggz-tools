        <name>OX2HV7M</name>
        <desc>Kumla Sjöpark by Radiola, Traditional Cache(2.0/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2HV7M</url>
        <urlname>Kumla Sjöpark</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2378712">
            <groundspeak:name>Kumla Sjöpark</groundspeak:name>
            <groundspeak:placed_by>Radiola</groundspeak:placed_by>
            <groundspeak:owner id="64761">Radiola</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
