        <groundspeak:cache archived="False" available="True" id="1876392">
            <groundspeak:name>Scouting for Treasure #14 – Rock and a Hard Place</groundspeak:name>
            <groundspeak:placed_by>tiggy00</groundspeak:placed_by>
            <groundspeak:owner id="1688">tiggy00</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
