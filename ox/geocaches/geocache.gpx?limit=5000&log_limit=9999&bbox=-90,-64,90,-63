        <name>OX1MDMA</name>
        <desc>Hay, Its Another Bluff Cache!! by fishingfanatic, Traditional Cache(4.0/4.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1MDMA</url>
        <urlname>Hay, Its Another Bluff Cache!!</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1532464">
            <groundspeak:name>Hay, Its Another Bluff Cache!!</groundspeak:name>
            <groundspeak:placed_by>fishingfanatic</groundspeak:placed_by>
            <groundspeak:owner id="3342">fishingfanatic</groundspeak:owner>
