        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1871424">
            <groundspeak:name>Scouting for Treasure #9 – Does your Cache Hang Lo</groundspeak:name>
            <groundspeak:placed_by>tiggy00</groundspeak:placed_by>
            <groundspeak:owner id="1688">tiggy00</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
