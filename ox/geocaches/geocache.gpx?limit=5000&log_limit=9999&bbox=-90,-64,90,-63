        <desc>Scouting for Treasure #3 – Want to Play a Game? by tiggy00, Traditional Cache(1.5/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX20XM3</url>
        <urlname>Scouting for Treasure #3 – Want to Play a Game?</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1874573">
            <groundspeak:name>Scouting for Treasure #3 – Want to Play a Game?</groundspeak:name>
            <groundspeak:placed_by>tiggy00</groundspeak:placed_by>
            <groundspeak:owner id="1688">tiggy00</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
