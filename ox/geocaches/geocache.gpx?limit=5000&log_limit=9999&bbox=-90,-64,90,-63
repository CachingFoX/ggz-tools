        <url>http://www.opencaching.com/#geocache/OXPJ7W</url>
        <urlname>Malagash Mine: The Wheels of Time</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="672944">
            <groundspeak:name>Malagash Mine: The Wheels of Time</groundspeak:name>
            <groundspeak:placed_by>Wallace-River</groundspeak:placed_by>
            <groundspeak:owner id="5497">Wallace-River</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
