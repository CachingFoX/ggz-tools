        <groundspeak:cache archived="False" available="True" id="1877426">
            <groundspeak:name>Scouting for Treasure #13 – Are you Confused?</groundspeak:name>
            <groundspeak:placed_by>tiggy00</groundspeak:placed_by>
            <groundspeak:owner id="1688">tiggy00</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
