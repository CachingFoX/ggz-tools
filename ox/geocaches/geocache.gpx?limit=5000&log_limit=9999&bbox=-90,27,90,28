        <desc>Dulce Et Decorum Est Pro Patria Mori by Carbon_Hunter, Traditional Cache(3.5/1.5/3.1)</desc>
        <url>http://www.opencaching.com/#geocache/OX1FDVQ</url>
        <urlname>Dulce Et Decorum Est Pro Patria Mori</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1383708">
            <groundspeak:name>Dulce Et Decorum Est Pro Patria Mori</groundspeak:name>
            <groundspeak:placed_by>Carbon_Hunter</groundspeak:placed_by>
            <groundspeak:owner id="3134">Carbon_Hunter</groundspeak:owner>
