            <groundspeak:name>Antitankgracht</groundspeak:name>
            <groundspeak:placed_by>KoenDR</groundspeak:placed_by>
            <groundspeak:owner id="17924">KoenDR</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
