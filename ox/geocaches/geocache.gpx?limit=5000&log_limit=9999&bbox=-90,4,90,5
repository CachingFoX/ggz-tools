        <name>OX3CN2Y</name>
        <desc>De Bakesteinpoort by Jurgen96, Traditional Cache(1.5/3.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX3CN2Y</url>
        <urlname>De Bakesteinpoort</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="3148327">
            <groundspeak:name>De Bakesteinpoort</groundspeak:name>
            <groundspeak:placed_by>Jurgen96</groundspeak:placed_by>
