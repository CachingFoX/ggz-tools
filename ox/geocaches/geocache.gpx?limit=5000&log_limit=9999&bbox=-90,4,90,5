        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2127231">
            <groundspeak:name>Cache boost 4 - DECEPTION POINT</groundspeak:name>
            <groundspeak:placed_by>YANKEEMIKE1</groundspeak:placed_by>
            <groundspeak:owner id="7517">YANKEEMIKE1</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
