        <groundspeak:cache archived="False" available="True" id="28464763">
            <groundspeak:name>WayPoint 12,5 jaar Moordrecht</groundspeak:name>
            <groundspeak:placed_by>WayPoint</groundspeak:placed_by>
            <groundspeak:owner id="128073">WayPoint</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
            <groundspeak:difficulty>2</groundspeak:difficulty>
