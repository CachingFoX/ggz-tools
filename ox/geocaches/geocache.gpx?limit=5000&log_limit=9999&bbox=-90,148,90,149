        <groundspeak:cache archived="False" available="True" id="1945660">
            <groundspeak:name>Hang in There at Pig Hill</groundspeak:name>
            <groundspeak:placed_by>RBIRTLES</groundspeak:placed_by>
            <groundspeak:owner id="2795">RBIRTLES</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
