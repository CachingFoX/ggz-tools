        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2362585">
            <groundspeak:name>On the Shoreline  - Geobiking Hueston Woods *</groundspeak:name>
            <groundspeak:placed_by>cache_n_out</groundspeak:placed_by>
            <groundspeak:owner id="12571">cache_n_out</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
