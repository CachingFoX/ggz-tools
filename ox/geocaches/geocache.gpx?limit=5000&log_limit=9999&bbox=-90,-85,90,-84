        <desc>Camp Dennison - Little Miami Bike Trail by cache_n_out, Traditional Cache(2.0/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1ZY1W</url>
        <urlname>Camp Dennison - Little Miami Bike Trail</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1845178">
            <groundspeak:name>Camp Dennison - Little Miami Bike Trail</groundspeak:name>
            <groundspeak:placed_by>cache_n_out</groundspeak:placed_by>
            <groundspeak:owner id="12571">cache_n_out</groundspeak:owner>
