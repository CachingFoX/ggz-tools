        <groundspeak:cache archived="False" available="True" id="3170004">
            <groundspeak:name>O culto do Espírito Santo</groundspeak:name>
            <groundspeak:placed_by>Team PortoJudeu</groundspeak:placed_by>
            <groundspeak:owner id="73061">Team PortoJudeu</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
