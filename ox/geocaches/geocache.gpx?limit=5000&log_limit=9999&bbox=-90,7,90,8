        <groundspeak:cache archived="False" available="True" id="2730761">
            <groundspeak:name>Gasselstiege</groundspeak:name>
            <groundspeak:placed_by>SCHNUCKIKUCKI</groundspeak:placed_by>
            <groundspeak:owner id="103837">SCHNUCKIKUCKI</groundspeak:owner>
