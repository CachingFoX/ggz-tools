        <groundspeak:cache archived="False" available="True" id="2779337">
            <groundspeak:name>Häuser der Helfer-Feuerwehr in Langenberg (reload)</groundspeak:name>
            <groundspeak:placed_by>unknown-lifeform</groundspeak:placed_by>
            <groundspeak:owner id="47546">unknown-lifeform</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
