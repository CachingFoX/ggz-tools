        <name>OX19JRZ</name>
        <desc>La Chapelle de Combes by Javelot, Unknown Cache(1.5/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX19JRZ</url>
        <urlname>La Chapelle de Combes</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="1209712">
            <groundspeak:name>La Chapelle de Combes</groundspeak:name>
            <groundspeak:placed_by>Javelot</groundspeak:placed_by>
            <groundspeak:owner id="443">Javelot</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
            <groundspeak:difficulty>1.5</groundspeak:difficulty>
