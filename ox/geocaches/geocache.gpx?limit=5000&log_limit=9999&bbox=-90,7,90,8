        <groundspeak:cache archived="False" available="True" id="1551962">
            <groundspeak:name>Zigeunerwiese</groundspeak:name>
            <groundspeak:placed_by>apfelmaus</groundspeak:placed_by>
            <groundspeak:owner id="4940">apfelmaus</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
