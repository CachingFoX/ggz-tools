        <time>2009-01-16T08:00:00.000Z</time>
        <name>OX1KTK1</name>
        <desc>Urfeld by GISMOGIZMO, Unknown Cache(3.0/1.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1KTK1</url>
        <urlname>Urfeld</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="1514165">
            <groundspeak:name>Urfeld</groundspeak:name>
            <groundspeak:placed_by>GISMOGIZMO</groundspeak:placed_by>
            <groundspeak:owner id="61254">GISMOGIZMO</groundspeak:owner>
