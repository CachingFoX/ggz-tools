        <groundspeak:cache archived="False" available="True" id="2108995">
            <groundspeak:name>Up &amp; Down im Fleyer Wald - I</groundspeak:name>
            <groundspeak:placed_by>BW.MGDP</groundspeak:placed_by>
            <groundspeak:owner id="426">BW.MGDP</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
