            <groundspeak:name>Tapetenfabrik</groundspeak:name>
            <groundspeak:placed_by>Baumhoernchen</groundspeak:placed_by>
            <groundspeak:owner id="52840">Baumhoernchen</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
