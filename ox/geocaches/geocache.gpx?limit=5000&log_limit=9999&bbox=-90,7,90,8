        <name>OX14E89</name>
        <desc>Kapelle Leschede by cache.me, Traditional Cache(1.0/1.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX14E89</url>
        <urlname>Kapelle Leschede</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1056396">
            <groundspeak:name>Kapelle Leschede</groundspeak:name>
            <groundspeak:placed_by>cache.me</groundspeak:placed_by>
            <groundspeak:owner id="3535">cache.me</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
