        <name>OX25CZ2</name>
        <desc>No way – der gemeine Bonus by GeoMops, Unknown Cache(5.0/3.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX25CZ2</url>
        <urlname>No way – der gemeine Bonus</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="2008461">
            <groundspeak:name>No way – der gemeine Bonus</groundspeak:name>
            <groundspeak:placed_by>GeoMops</groundspeak:placed_by>
            <groundspeak:owner id="5474">GeoMops</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
