        <groundspeak:cache archived="False" available="True" id="3480358">
            <groundspeak:name>Life after people - 10 years</groundspeak:name>
            <groundspeak:placed_by>GeoJediMeister</groundspeak:placed_by>
            <groundspeak:owner id="33586">GeoJediMeister</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
