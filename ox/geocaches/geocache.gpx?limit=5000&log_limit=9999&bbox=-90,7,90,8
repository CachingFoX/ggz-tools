        <groundspeak:cache archived="False" available="True" id="845649">
            <groundspeak:name>Burgschatz</groundspeak:name>
            <groundspeak:placed_by>hedgeway</groundspeak:placed_by>
            <groundspeak:owner id="505">hedgeway</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
