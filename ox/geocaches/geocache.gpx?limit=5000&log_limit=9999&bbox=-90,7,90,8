        <desc>FELDLACH by FIL67, Multi-cache(2.0/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1K7DM</url>
        <urlname>FELDLACH</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="1496700">
            <groundspeak:name>FELDLACH</groundspeak:name>
            <groundspeak:placed_by>FIL67</groundspeak:placed_by>
            <groundspeak:owner id="42998">FIL67</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
