        <groundspeak:cache archived="False" available="True" id="2123788">
            <groundspeak:name>Westbevern - Krinkhütte </groundspeak:name>
            <groundspeak:placed_by>ThiloG</groundspeak:placed_by>
            <groundspeak:owner id="6125">ThiloG</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
