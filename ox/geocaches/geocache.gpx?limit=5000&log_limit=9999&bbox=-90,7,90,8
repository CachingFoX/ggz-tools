        <name>OX37FJV</name>
        <desc>Eschendorf TB Hotel by BMP007, Multi-cache(2.0/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX37FJV</url>
        <urlname>Eschendorf TB Hotel</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="2994099">
            <groundspeak:name>Eschendorf TB Hotel</groundspeak:name>
            <groundspeak:placed_by>BMP007</groundspeak:placed_by>
            <groundspeak:owner id="72872">BMP007</groundspeak:owner>
