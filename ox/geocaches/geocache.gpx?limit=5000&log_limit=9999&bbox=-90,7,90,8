        <name>OX2FH2Q</name>
        <desc>Der mit den Vögeln spricht by BSIEBERTZ, Unknown Cache(1.0/1.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2FH2Q</url>
        <urlname>Der mit den Vögeln spricht</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="2310329">
            <groundspeak:name>Der mit den Vögeln spricht</groundspeak:name>
            <groundspeak:placed_by>BSIEBERTZ</groundspeak:placed_by>
            <groundspeak:owner id="14747">BSIEBERTZ</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
