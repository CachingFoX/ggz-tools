        <groundspeak:cache archived="False" available="True" id="3104963">
            <groundspeak:name>Willkommen in...Schürdt</groundspeak:name>
            <groundspeak:placed_by>SOL-DE</groundspeak:placed_by>
            <groundspeak:owner id="10836">SOL-DE</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
