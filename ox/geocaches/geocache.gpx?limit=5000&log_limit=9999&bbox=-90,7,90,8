        <url>http://www.opencaching.com/#geocache/OX1MNM7</url>
        <urlname>Im Umlauf</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="1540149">
            <groundspeak:name>Im Umlauf</groundspeak:name>
            <groundspeak:placed_by>BrianEquator</groundspeak:placed_by>
            <groundspeak:owner id="1597">BrianEquator</groundspeak:owner>
