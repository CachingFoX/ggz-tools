        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="1574931">
            <groundspeak:name>Kyloe</groundspeak:name>
            <groundspeak:placed_by>Mapau</groundspeak:placed_by>
            <groundspeak:owner id="14205">Mapau</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
