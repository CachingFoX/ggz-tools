        <groundspeak:cache archived="False" available="True" id="2352194">
            <groundspeak:name>DE-KL-GB-Sasquatch's Lair</groundspeak:name>
            <groundspeak:placed_by>GeoJediMeister</groundspeak:placed_by>
            <groundspeak:owner id="33586">GeoJediMeister</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
