        <groundspeak:cache archived="False" available="True" id="2009522">
            <groundspeak:name>Cabo Mondego Jurássico</groundspeak:name>
            <groundspeak:placed_by>Necas</groundspeak:placed_by>
            <groundspeak:owner id="2386">Necas</groundspeak:owner>
