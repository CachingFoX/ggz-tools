            <groundspeak:name>Apanha-me se puderes… Rotundas há muitas...</groundspeak:name>
            <groundspeak:placed_by>joaomanuelcoelho</groundspeak:placed_by>
            <groundspeak:owner id="8149">joaomanuelcoelho</groundspeak:owner>
