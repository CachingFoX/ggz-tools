        <name>OX29PEJ</name>
        <desc>Quase via Pardelhas by musa, Traditional Cache(2.0/3.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX29PEJ</url>
        <urlname>Quase via Pardelhas</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2136755">
            <groundspeak:name>Quase via Pardelhas</groundspeak:name>
            <groundspeak:placed_by>musa</groundspeak:placed_by>
            <groundspeak:owner id="6910">musa</groundspeak:owner>
