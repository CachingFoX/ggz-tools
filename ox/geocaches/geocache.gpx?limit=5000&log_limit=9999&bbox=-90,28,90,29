        <groundspeak:cache archived="False" available="True" id="1237880">
            <groundspeak:name>Low Range In</groundspeak:name>
            <groundspeak:placed_by>gerhardoosMPSa</groundspeak:placed_by>
            <groundspeak:owner id="4519">gerhardoosMPSa</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
