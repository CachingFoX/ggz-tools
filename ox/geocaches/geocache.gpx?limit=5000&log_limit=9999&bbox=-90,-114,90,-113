        <groundspeak:cache archived="False" available="True" id="28476578">
            <groundspeak:name>Running Eagle Falls - Glacier National Park</groundspeak:name>
            <groundspeak:placed_by>-Orion-</groundspeak:placed_by>
            <groundspeak:owner id="4132">-Orion-</groundspeak:owner>
            <groundspeak:type>Virtual Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
