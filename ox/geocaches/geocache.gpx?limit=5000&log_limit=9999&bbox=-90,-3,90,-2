        <name>OXMGHN</name>
        <desc>Wholehope Still by MOSS TROOPER371, Traditional Cache(1.5/3.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXMGHN</url>
        <urlname>Wholehope Still</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="611744">
            <groundspeak:name>Wholehope Still</groundspeak:name>
