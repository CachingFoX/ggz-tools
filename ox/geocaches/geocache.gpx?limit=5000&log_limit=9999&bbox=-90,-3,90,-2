        <url>http://www.opencaching.com/#geocache/OX2H59K</url>
        <urlname>Black Gold : The Heugh</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2358592">
            <groundspeak:name>Black Gold : The Heugh</groundspeak:name>
