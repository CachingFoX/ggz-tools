            <groundspeak:name>Ar milin Merlet</groundspeak:name>
            <groundspeak:placed_by>Nixul</groundspeak:placed_by>
            <groundspeak:owner id="128579">Nixul</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
