        <groundspeak:cache archived="False" available="True" id="1812837">
            <groundspeak:name>BlazeAway</groundspeak:name>
            <groundspeak:placed_by>petergpstraining</groundspeak:placed_by>
            <groundspeak:owner id="3286">petergpstraining</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
