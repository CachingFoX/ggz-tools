        <groundspeak:cache archived="False" available="True" id="1445637">
            <groundspeak:name>Alphabeticus Viperion</groundspeak:name>
            <groundspeak:placed_by>Guanajuato</groundspeak:placed_by>
            <groundspeak:owner id="3638">Guanajuato</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
