        <url>http://www.opencaching.com/#geocache/OXZTMX8</url>
        <urlname>OZONE</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="28470501">
            <groundspeak:name>OZONE</groundspeak:name>
            <groundspeak:placed_by>kacher</groundspeak:placed_by>
            <groundspeak:owner id="79909">kacher</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
