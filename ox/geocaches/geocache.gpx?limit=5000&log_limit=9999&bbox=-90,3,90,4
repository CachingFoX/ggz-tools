        <groundspeak:cache archived="False" available="True" id="2944281">
            <groundspeak:name>La Velittorale</groundspeak:name>
            <groundspeak:placed_by>cargol</groundspeak:placed_by>
            <groundspeak:owner id="85474">cargol</groundspeak:owner>
