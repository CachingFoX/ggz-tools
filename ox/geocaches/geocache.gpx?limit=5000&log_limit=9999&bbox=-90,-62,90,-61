            <groundspeak:name>Whycocomagh Provincial Park</groundspeak:name>
            <groundspeak:placed_by>ACGA</groundspeak:placed_by>
            <groundspeak:owner id="600">ACGA</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
