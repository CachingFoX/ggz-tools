        <name>OX1PZ84</name>
        <desc>Peanut's Playground by FrogStomper, Multi-cache(3.2/3.6/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1PZ84</url>
        <urlname>Peanut's Playground</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="1608005">
            <groundspeak:name>Peanut's Playground</groundspeak:name>
            <groundspeak:placed_by>FrogStomper</groundspeak:placed_by>
            <groundspeak:owner id="4672">FrogStomper</groundspeak:owner>
