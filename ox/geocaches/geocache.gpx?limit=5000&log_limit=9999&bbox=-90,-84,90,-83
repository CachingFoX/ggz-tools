        <urlname>Tree I.D. 03 - Sycamore</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1779715">
            <groundspeak:name>Tree I.D. 03 - Sycamore</groundspeak:name>
            <groundspeak:placed_by>FrogStomper</groundspeak:placed_by>
            <groundspeak:owner id="4672">FrogStomper</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
