        <desc>Wesley Chapel by creekstompers, Traditional Cache(2.0/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1P7N6</url>
        <urlname>Wesley Chapel</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1586307">
            <groundspeak:name>Wesley Chapel</groundspeak:name>
            <groundspeak:placed_by>creekstompers</groundspeak:placed_by>
            <groundspeak:owner id="7601">creekstompers</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
            <groundspeak:difficulty>2</groundspeak:difficulty>
