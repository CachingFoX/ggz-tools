        <groundspeak:cache archived="False" available="True" id="28469984">
            <groundspeak:name>A view to the sea</groundspeak:name>
            <groundspeak:placed_by>littlenorm</groundspeak:placed_by>
            <groundspeak:owner id="81628">littlenorm</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
