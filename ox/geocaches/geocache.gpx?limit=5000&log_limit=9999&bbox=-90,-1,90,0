        <name>OXZTBQE</name>
        <desc>La creueta by BICHEJOTE600, Traditional Cache(1.1/1.2/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTBQE</url>
        <urlname>La creueta</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="28461703">
            <groundspeak:name>La creueta</groundspeak:name>
            <groundspeak:placed_by>BICHEJOTE600</groundspeak:placed_by>
            <groundspeak:owner id="123836">BICHEJOTE600</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
