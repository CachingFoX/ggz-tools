        <name>OX3JH4Y</name>
        <desc>Palais de justice by Calimero33, Traditional Cache(1.5/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX3JH4Y</url>
        <urlname>Palais de justice</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="3323291">
            <groundspeak:name>Palais de justice</groundspeak:name>
            <groundspeak:placed_by>Calimero33</groundspeak:placed_by>
            <groundspeak:owner id="128753">Calimero33</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
