        <desc>Trinity Square by MAROHO, Traditional Cache(2.0/1.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2F8GE</url>
        <urlname>Trinity Square</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2302105">
            <groundspeak:name>Trinity Square</groundspeak:name>
            <groundspeak:placed_by>MAROHO</groundspeak:placed_by>
            <groundspeak:owner id="6704">MAROHO</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
