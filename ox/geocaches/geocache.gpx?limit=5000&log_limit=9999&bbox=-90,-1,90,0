        <urlname>National Trust Trail - Cowbar Nab</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="2462433">
            <groundspeak:name>National Trust Trail - Cowbar Nab</groundspeak:name>
            <groundspeak:placed_by>National Trust</groundspeak:placed_by>
