        <time>2012-11-14T19:57:03.000Z</time>
        <name>OXZTBPW</name>
        <desc>La Creu de Pedra by BICHEJOTE600, Traditional Cache(1.8/1.4/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTBPW</url>
        <urlname>La Creu de Pedra</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="28461685">
            <groundspeak:name>La Creu de Pedra</groundspeak:name>
            <groundspeak:placed_by>BICHEJOTE600</groundspeak:placed_by>
            <groundspeak:owner id="123836">BICHEJOTE600</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
