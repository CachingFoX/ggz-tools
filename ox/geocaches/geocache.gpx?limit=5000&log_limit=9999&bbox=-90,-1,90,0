        <name>OXZTRNN</name>
        <desc>Wimpole Estate NT Cache 3 by Wimpole Estate NT, Traditional Cache(3.2/3.1/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTRNN</url>
        <urlname>Wimpole Estate NT Cache 3</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="28474141">
            <groundspeak:name>Wimpole Estate NT Cache 3</groundspeak:name>
