        <name>OX2G7B9</name>
        <desc>MK Schooldays: 10km Race by MKGees, Unknown Cache(4.0/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2G7B9</url>
        <urlname>MK Schooldays: 10km Race</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="2330775">
            <groundspeak:name>MK Schooldays: 10km Race</groundspeak:name>
            <groundspeak:placed_by>MKGees</groundspeak:placed_by>
            <groundspeak:owner id="50322">MKGees</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
