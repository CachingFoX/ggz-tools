        <name>OX3JJEG</name>
        <desc>Place de la Victoire by Calimero33, Traditional Cache(1.5/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX3JJEG</url>
        <urlname>Place de la Victoire</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="3324549">
            <groundspeak:name>Place de la Victoire</groundspeak:name>
            <groundspeak:placed_by>Calimero33</groundspeak:placed_by>
            <groundspeak:owner id="128753">Calimero33</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
