            <groundspeak:name>Slippery Slope</groundspeak:name>
            <groundspeak:placed_by>The Machman</groundspeak:placed_by>
            <groundspeak:owner id="12235">The Machman</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
