        <name>OXZTE3Y</name>
        <desc>El edificio más antiguo de Huelva by nakamateamhuelva, Traditional Cache(1.0/1.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTE3Y</url>
        <urlname>El edificio más antiguo de Huelva</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="28463981">
            <groundspeak:name>El edificio más antiguo de Huelva</groundspeak:name>
            <groundspeak:placed_by>nakamateamhuelva</groundspeak:placed_by>
            <groundspeak:owner id="137395">nakamateamhuelva</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
            <groundspeak:difficulty>1</groundspeak:difficulty>
