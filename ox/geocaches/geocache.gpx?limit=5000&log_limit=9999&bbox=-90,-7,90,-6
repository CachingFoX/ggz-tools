    <wpt lon="-6.95145" lat="37.2535">
        <time>2012-07-28T12:16:23.000Z</time>
        <name>OXZTE3W</name>
        <desc>Estación de tren by nakamateamhuelva, Traditional Cache(1.0/1.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTE3W</url>
        <urlname>Estación de tren</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="28463979">
            <groundspeak:name>Estación de tren</groundspeak:name>
            <groundspeak:placed_by>nakamateamhuelva</groundspeak:placed_by>
            <groundspeak:owner id="137395">nakamateamhuelva</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
