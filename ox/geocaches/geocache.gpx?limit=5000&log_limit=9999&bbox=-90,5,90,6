        <groundspeak:cache archived="False" available="True" id="3105612">
            <groundspeak:name>#3 La conciergerie du Château de Jean d'Heurs</groundspeak:name>
            <groundspeak:placed_by>VOUCHT</groundspeak:placed_by>
            <groundspeak:owner id="99379">VOUCHT</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
