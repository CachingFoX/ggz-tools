        <name>OX1EXDD</name>
        <desc>Le pont noir by DOURET, Traditional Cache(2.5/1.5/3.1)</desc>
        <url>http://www.opencaching.com/#geocache/OX1EXDD</url>
        <urlname>Le pont noir</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1367919">
            <groundspeak:name>Le pont noir</groundspeak:name>
            <groundspeak:placed_by>DOURET</groundspeak:placed_by>
            <groundspeak:owner id="3150">DOURET</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
