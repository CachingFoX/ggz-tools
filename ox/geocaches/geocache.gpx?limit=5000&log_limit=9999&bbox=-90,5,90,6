        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1502780">
            <groundspeak:name>Le Château de Montfort</groundspeak:name>
            <groundspeak:placed_by>gb51</groundspeak:placed_by>
