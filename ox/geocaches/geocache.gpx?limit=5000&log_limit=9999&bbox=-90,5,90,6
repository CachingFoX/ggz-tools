        <name>OXZTY12</name>
        <desc>La Corniche by equipe_rabatau, Traditional Cache(1.9/1.4/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTY12</url>
        <urlname>La Corniche</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="28478307">
            <groundspeak:name>La Corniche</groundspeak:name>
            <groundspeak:placed_by>equipe_rabatau</groundspeak:placed_by>
            <groundspeak:owner id="2950">equipe_rabatau</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
