        <name>OXNCFE</name>
        <desc>guldenberg by Purk, Multi-cache(3.0/3.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXNCFE</url>
        <urlname>guldenberg</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="637622">
            <groundspeak:name>guldenberg</groundspeak:name>
            <groundspeak:placed_by>Purk</groundspeak:placed_by>
