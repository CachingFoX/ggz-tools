        <groundspeak:cache archived="False" available="True" id="1862999">
            <groundspeak:name>Grandcourt</groundspeak:name>
            <groundspeak:placed_by>daru2</groundspeak:placed_by>
            <groundspeak:owner id="3150">daru2</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
