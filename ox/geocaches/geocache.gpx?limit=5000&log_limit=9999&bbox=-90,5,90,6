        <url>http://www.opencaching.com/#geocache/OX3WCWB</url>
        <urlname>[E-diots] Batracien</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="3587300">
            <groundspeak:name>[E-diots] Batracien</groundspeak:name>
