        <groundspeak:cache archived="False" available="True" id="976130">
            <groundspeak:name>Wat is het mooi op de Veluwe </groundspeak:name>
            <groundspeak:placed_by>human-care</groundspeak:placed_by>
            <groundspeak:owner id="4178">human-care</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
            <groundspeak:difficulty>3.5</groundspeak:difficulty>
