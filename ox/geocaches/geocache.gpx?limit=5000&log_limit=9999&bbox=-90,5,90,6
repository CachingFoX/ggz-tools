        <name>OX2FJ8V</name>
        <desc>&quot;Een korte onderbreking&quot; in de Heere Peel by ROBENADA, Multi-cache(2.5/2.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2FJ8V</url>
        <urlname>&quot;Een korte onderbreking&quot; in de Heere Peel</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="2311479">
            <groundspeak:name>&quot;Een korte onderbreking&quot; in de Heere Peel</groundspeak:name>
