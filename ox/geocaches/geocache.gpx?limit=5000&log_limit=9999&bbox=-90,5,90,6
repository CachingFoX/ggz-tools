        <desc>14/18 &quot;Les Soldats Belges&quot; by Tournesol 73, Unknown Cache(2.0/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX36Z3X</url>
        <urlname>14/18 &quot;Les Soldats Belges&quot;</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="2978260">
            <groundspeak:name>14/18 &quot;Les Soldats Belges&quot;</groundspeak:name>
            <groundspeak:placed_by>Tournesol 73</groundspeak:placed_by>
            <groundspeak:owner id="92335">Tournesol 73</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
            <groundspeak:difficulty>2</groundspeak:difficulty>
