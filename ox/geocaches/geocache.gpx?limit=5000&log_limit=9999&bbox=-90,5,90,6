        <name>OX2N8M1</name>
        <desc>De snelle sneltram by ICEVOGELKE, Traditional Cache(1.5/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2N8M1</url>
        <urlname>De snelle sneltram</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2480962">
            <groundspeak:name>De snelle sneltram</groundspeak:name>
            <groundspeak:placed_by>ICEVOGELKE</groundspeak:placed_by>
            <groundspeak:owner id="32350">ICEVOGELKE</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
