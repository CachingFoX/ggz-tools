                <ox:awesomeness>3.0</ox:awesomeness>
                <ox:difficulty>1.5</ox:difficulty>
                <ox:size>4.0</ox:size>
                <ox:terrain>1.0</ox:terrain>
            </ox:ratings>
        </ox:opencaching>
