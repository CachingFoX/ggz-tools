        <groundspeak:cache archived="False" available="True" id="2523690">
            <groundspeak:name>TDT09: The Derbyshire Trails</groundspeak:name>
            <groundspeak:placed_by>Ve8-UK</groundspeak:placed_by>
            <groundspeak:owner id="2121">Ve8-UK</groundspeak:owner>
