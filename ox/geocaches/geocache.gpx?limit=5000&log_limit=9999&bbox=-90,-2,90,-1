        <name>OX2M4HN</name>
        <desc>National Trust Trail - Cliff Ridge Wood &amp; Quarry by National Trust, Multi-cache(2.5/2.4/2.9)</desc>
        <url>http://www.opencaching.com/#geocache/OX2M4HN</url>
        <urlname>National Trust Trail - Cliff Ridge Wood &amp; Quarry</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="2447254">
            <groundspeak:name>National Trust Trail - Cliff Ridge Wood &amp; Quarry</groundspeak:name>
            <groundspeak:placed_by>National Trust</groundspeak:placed_by>
            <groundspeak:owner id="43157">National Trust</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
            <groundspeak:difficulty>2.5</groundspeak:difficulty>
