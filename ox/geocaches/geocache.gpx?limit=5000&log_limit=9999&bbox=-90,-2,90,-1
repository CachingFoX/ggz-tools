        <url>http://www.opencaching.com/#geocache/OX1Y97H</url>
        <urlname>The Monarch's Way - Hursley</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="1796343">
            <groundspeak:name>The Monarch's Way - Hursley</groundspeak:name>
            <groundspeak:placed_by>harryatkins</groundspeak:placed_by>
            <groundspeak:owner id="14019">harryatkins</groundspeak:owner>
