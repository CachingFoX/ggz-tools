        <time>2011-05-24T07:00:00.000Z</time>
        <name>OX2WW95</name>
        <desc>National Trust Trail - Penshaw Monument by National Trust, Multi-cache(2.5/2.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2WW95</url>
        <urlname>National Trust Trail - Penshaw Monument</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="2677630">
            <groundspeak:name>National Trust Trail - Penshaw Monument</groundspeak:name>
            <groundspeak:placed_by>National Trust</groundspeak:placed_by>
            <groundspeak:owner id="43157">National Trust</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
