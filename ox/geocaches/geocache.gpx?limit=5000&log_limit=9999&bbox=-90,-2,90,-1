        <name>OX2PP54</name>
        <desc>TDT16: The Derbyshire Trails by Ve8-UK, Traditional Cache(1.5/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2PP54</url>
        <urlname>TDT16: The Derbyshire Trails</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2523745">
            <groundspeak:name>TDT16: The Derbyshire Trails</groundspeak:name>
            <groundspeak:placed_by>Ve8-UK</groundspeak:placed_by>
            <groundspeak:owner id="2121">Ve8-UK</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
