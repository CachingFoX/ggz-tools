        <name>OX2PP07</name>
        <desc>TDT01: The Derbyshire Trails by Ve8-UK, Traditional Cache(1.5/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2PP07</url>
        <urlname>TDT01: The Derbyshire Trails</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2523593">
            <groundspeak:name>TDT01: The Derbyshire Trails</groundspeak:name>
            <groundspeak:placed_by>Ve8-UK</groundspeak:placed_by>
            <groundspeak:owner id="2121">Ve8-UK</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
