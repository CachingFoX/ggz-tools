        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2776958">
            <groundspeak:name>National Trust Cache - Hawthorn Towers</groundspeak:name>
            <groundspeak:placed_by>National Trust</groundspeak:placed_by>
            <groundspeak:owner id="43157">National Trust</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
