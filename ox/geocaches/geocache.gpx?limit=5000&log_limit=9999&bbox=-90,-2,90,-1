        <groundspeak:cache archived="False" available="True" id="2558849">
            <groundspeak:name>National Trust Dark Peak 4</groundspeak:name>
            <groundspeak:placed_by>National Trust Peak District</groundspeak:placed_by>
            <groundspeak:owner id="23193">National Trust Peak District</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
