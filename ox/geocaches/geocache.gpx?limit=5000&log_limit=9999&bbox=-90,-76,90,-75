        <groundspeak:cache archived="False" available="True" id="1661102">
            <groundspeak:name>MMC 10: Eeny, Meaney, Miny, Moe</groundspeak:name>
            <groundspeak:placed_by>apex</groundspeak:placed_by>
            <groundspeak:owner id="6908">apex</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
