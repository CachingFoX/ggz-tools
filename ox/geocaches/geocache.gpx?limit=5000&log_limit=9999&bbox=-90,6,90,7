            <groundspeak:name>Mia unua trezoro - Animaloparko</groundspeak:name>
            <groundspeak:placed_by>Huck-Kroy</groundspeak:placed_by>
            <groundspeak:owner id="25008">Huck-Kroy</groundspeak:owner>
