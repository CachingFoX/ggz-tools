        <groundspeak:cache archived="False" available="True" id="1564974">
            <groundspeak:name>Kreuzweg Petrisberg</groundspeak:name>
            <groundspeak:placed_by>fidepus</groundspeak:placed_by>
            <groundspeak:owner id="5214">fidepus</groundspeak:owner>
