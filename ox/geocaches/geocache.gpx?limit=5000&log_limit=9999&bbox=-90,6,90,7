        <name>OXZTRW3</name>
        <desc>De oude vijver by hahu128, Traditional Cache(2.1/2.9/2.9)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTRW3</url>
        <urlname>De oude vijver</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="28474309">
            <groundspeak:name>De oude vijver</groundspeak:name>
            <groundspeak:placed_by>hahu128</groundspeak:placed_by>
