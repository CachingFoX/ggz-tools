        <name>OX228Z4</name>
        <desc>Assen 'de Bosrand' by hahu38, Traditional Cache(2.7/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX228Z4</url>
        <urlname>Assen 'de Bosrand'</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1915246">
            <groundspeak:name>Assen 'de Bosrand'</groundspeak:name>
            <groundspeak:placed_by>hahu38</groundspeak:placed_by>
            <groundspeak:owner id="37768">hahu38</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
