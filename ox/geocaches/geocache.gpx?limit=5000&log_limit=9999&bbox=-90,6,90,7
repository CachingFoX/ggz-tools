        <time>2011-08-06T11:56:35.000-05:00</time>
        <name>OXZTREA</name>
        <desc>Trümmernberg by Iorek Byrnison, Traditional Cache(2.4/2.9/2.9)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTREA</url>
        <urlname>Trümmernberg</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="28473913">
            <groundspeak:name>Trümmernberg</groundspeak:name>
            <groundspeak:placed_by>Iorek Byrnison</groundspeak:placed_by>
            <groundspeak:owner id="50511">Iorek Byrnison</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
