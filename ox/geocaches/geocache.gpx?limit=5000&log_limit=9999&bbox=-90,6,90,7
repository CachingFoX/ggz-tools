        <groundspeak:cache archived="False" available="True" id="2638574">
            <groundspeak:name>Boxenstop - Das Qualifying</groundspeak:name>
            <groundspeak:placed_by>SharkAttack</groundspeak:placed_by>
            <groundspeak:owner id="2511">SharkAttack</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
