        <urlname>Mirco´s Micro´s 4 - Die Insel</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2735620">
            <groundspeak:name>Mirco´s Micro´s 4 - Die Insel</groundspeak:name>
            <groundspeak:placed_by>teamaix</groundspeak:placed_by>
