        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="3244699">
            <groundspeak:name>B268 Allee Münchweiler</groundspeak:name>
            <groundspeak:placed_by>Saarfleet Commander</groundspeak:placed_by>
            <groundspeak:owner id="58144">Saarfleet Commander</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
