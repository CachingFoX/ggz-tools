        <groundspeak:cache archived="False" available="True" id="28474340">
            <groundspeak:name>De freule van Zuidlaarderveen</groundspeak:name>
            <groundspeak:placed_by>hahu128</groundspeak:placed_by>
            <groundspeak:owner id="46072">hahu128</groundspeak:owner>
