        <time>2012-05-05T07:00:00.000Z</time>
        <name>OX3HG20</name>
        <desc>Der Weltkrieg war vor deiner Tür - Die Saarstrecke by DIE_GEOCRAESHER, Traditional Cache(1.5/2.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX3HG20</url>
        <urlname>Der Weltkrieg war vor deiner Tür - Die Saarstrecke</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="3292448">
            <groundspeak:name>Der Weltkrieg war vor deiner Tür - Die Saarstrecke</groundspeak:name>
            <groundspeak:placed_by>DIE_GEOCRAESHER</groundspeak:placed_by>
            <groundspeak:owner id="11794">DIE_GEOCRAESHER</groundspeak:owner>
