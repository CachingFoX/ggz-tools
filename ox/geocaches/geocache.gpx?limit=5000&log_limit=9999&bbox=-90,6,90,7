        <urlname>Les batraciens de la Mirande</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="2669030">
            <groundspeak:name>Les batraciens de la Mirande</groundspeak:name>
            <groundspeak:placed_by>gb51</groundspeak:placed_by>
            <groundspeak:owner id="133296">gb51</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
