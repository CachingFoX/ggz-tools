        <name>OX25N31</name>
        <desc>Brummer 4 - Lost little valley by gerhardoosMPSa, Traditional Cache(2.5/3.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX25N31</url>
        <urlname>Brummer 4 - Lost little valley</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2016272">
            <groundspeak:name>Brummer 4 - Lost little valley</groundspeak:name>
            <groundspeak:placed_by>gerhardoosMPSa</groundspeak:placed_by>
            <groundspeak:owner id="4519">gerhardoosMPSa</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
