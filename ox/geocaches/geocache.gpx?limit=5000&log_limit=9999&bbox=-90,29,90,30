        <groundspeak:cache archived="False" available="True" id="28463926">
            <groundspeak:name>Пенаты</groundspeak:name>
            <groundspeak:placed_by>HyLog</groundspeak:placed_by>
            <groundspeak:owner id="138213">HyLog</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
