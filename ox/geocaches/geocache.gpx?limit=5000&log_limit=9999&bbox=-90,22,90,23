        <desc>Hajoamisia by Ncore, Unknown Cache(4.0/2.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX338KR</url>
        <urlname>Hajoamisia</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="2868237">
            <groundspeak:name>Hajoamisia</groundspeak:name>
            <groundspeak:placed_by>Ncore</groundspeak:placed_by>
            <groundspeak:owner id="54667">Ncore</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
