        <name>OX1PFNB</name>
        <desc>Rooikransies by gerhardoosMPSa, Traditional Cache(2.0/3.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1PFNB</url>
        <urlname>Rooikransies</urlname>
        <sym>Geocache</sym>
