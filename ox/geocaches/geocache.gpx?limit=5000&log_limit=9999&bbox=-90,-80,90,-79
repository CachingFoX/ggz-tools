        <name>OX276P2</name>
        <desc>Remember the Commerce by all-hat, Traditional Cache(2.0/1.6/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX276P2</url>
        <urlname>Remember the Commerce</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2062029">
            <groundspeak:name>Remember the Commerce</groundspeak:name>
            <groundspeak:placed_by>all-hat</groundspeak:placed_by>
