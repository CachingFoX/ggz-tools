        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="2029302">
            <groundspeak:name>NCT - Nighttime walk, gopher it!</groundspeak:name>
            <groundspeak:placed_by>SPARTICUS</groundspeak:placed_by>
            <groundspeak:owner id="7539">SPARTICUS</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
