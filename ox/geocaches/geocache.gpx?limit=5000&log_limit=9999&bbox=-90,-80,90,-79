        <name>OX2G7G4</name>
        <desc>The Voice of Color by Hellnite, Unknown Cache(2.3/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2G7G4</url>
        <urlname>The Voice of Color</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="2330925">
            <groundspeak:name>The Voice of Color</groundspeak:name>
