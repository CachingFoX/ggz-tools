        <url>http://www.opencaching.com/#geocache/OX2FHY9</url>
        <urlname>BFL Boot Camp V :: The Illuminatrix</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="2311152">
            <groundspeak:name>BFL Boot Camp V :: The Illuminatrix</groundspeak:name>
            <groundspeak:placed_by>northernpenguin</groundspeak:placed_by>
