            <groundspeak:name>Willson's cemetery</groundspeak:name>
            <groundspeak:placed_by>all-hat</groundspeak:placed_by>
            <groundspeak:owner id="9764">all-hat</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
