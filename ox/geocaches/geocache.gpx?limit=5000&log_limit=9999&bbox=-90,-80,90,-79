        <desc>Beautiful Oakville Waterfront by res2100, Multi-cache(3.2/1.6/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTE0D</url>
        <urlname>Beautiful Oakville Waterfront</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="28463872">
            <groundspeak:name>Beautiful Oakville Waterfront</groundspeak:name>
            <groundspeak:placed_by>res2100</groundspeak:placed_by>
            <groundspeak:owner id="4660">res2100</groundspeak:owner>
