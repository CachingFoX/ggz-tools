        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="1945359">
            <groundspeak:name>GHMGC A Cache A Day - OCTOBER - Mini Challenge</groundspeak:name>
            <groundspeak:placed_by>northernpenguin</groundspeak:placed_by>
            <groundspeak:owner id="826">northernpenguin</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
