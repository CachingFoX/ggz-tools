        <urlname>Joshua (6 of 66)</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="28474317">
            <groundspeak:name>Joshua (6 of 66)</groundspeak:name>
            <groundspeak:placed_by>sunbeambatteries</groundspeak:placed_by>
            <groundspeak:owner id="42222">sunbeambatteries</groundspeak:owner>
