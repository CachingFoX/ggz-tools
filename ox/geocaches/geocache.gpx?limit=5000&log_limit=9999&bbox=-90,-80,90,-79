                    <groundspeak:date>2011-12-26T13:38:00.000Z</groundspeak:date>
                    <groundspeak:type>Found it</groundspeak:type>
                    <groundspeak:finder id="78371">steve.ver</groundspeak:finder>
                    <groundspeak:text encoded="False">&quot;&quot;</groundspeak:text>
