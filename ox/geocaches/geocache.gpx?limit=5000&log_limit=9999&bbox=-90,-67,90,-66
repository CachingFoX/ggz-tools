        <groundspeak:cache archived="False" available="True" id="2135374">
            <groundspeak:name>Plaza de Arecibo</groundspeak:name>
            <groundspeak:placed_by>ZEQ5</groundspeak:placed_by>
            <groundspeak:owner id="8977">ZEQ5</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
