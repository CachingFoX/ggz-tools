        <url>http://www.opencaching.com/#geocache/OX1ZPC0</url>
        <urlname>Sedan Broken Dreams</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1838765">
            <groundspeak:name>Sedan Broken Dreams</groundspeak:name>
            <groundspeak:placed_by>FIRESAFE</groundspeak:placed_by>
            <groundspeak:owner id="921">FIRESAFE</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
