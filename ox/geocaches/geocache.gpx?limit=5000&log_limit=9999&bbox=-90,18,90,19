        <name>OX3KCFC</name>
        <desc>Water tower by BiharyG, Traditional Cache(2.0/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX3KCFC</url>
        <urlname>Water tower</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="3348601">
            <groundspeak:name>Water tower</groundspeak:name>
            <groundspeak:placed_by>BiharyG</groundspeak:placed_by>
            <groundspeak:owner id="71494">BiharyG</groundspeak:owner>
