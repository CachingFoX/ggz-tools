        <name>OX3KECM</name>
        <desc>Sió by BiharyG, Traditional Cache(2.0/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX3KECM</url>
        <urlname>Sió</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="3350438">
            <groundspeak:name>Sió</groundspeak:name>
            <groundspeak:placed_by>BiharyG</groundspeak:placed_by>
            <groundspeak:owner id="71494">BiharyG</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
            <groundspeak:difficulty>2</groundspeak:difficulty>
