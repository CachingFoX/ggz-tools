            <groundspeak:name>piliky</groundspeak:name>
            <groundspeak:placed_by>jikra</groundspeak:placed_by>
            <groundspeak:owner id="2409">jikra</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
