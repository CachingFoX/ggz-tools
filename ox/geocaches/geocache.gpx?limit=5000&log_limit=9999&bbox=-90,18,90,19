        <name>OX3KCEM</name>
        <desc>Kálmán Imre by BiharyG, Traditional Cache(2.0/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX3KCEM</url>
        <urlname>Kálmán Imre</urlname>
        <sym>Geocache</sym>
