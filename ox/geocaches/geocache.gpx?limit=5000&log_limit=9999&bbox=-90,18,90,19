        <groundspeak:cache archived="False" available="True" id="28476426">
            <groundspeak:name>Zoborské jazierko</groundspeak:name>
            <groundspeak:placed_by>Titlak</groundspeak:placed_by>
            <groundspeak:owner id="22464">Titlak</groundspeak:owner>
