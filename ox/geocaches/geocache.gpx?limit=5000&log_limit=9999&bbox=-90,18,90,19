        <desc>Klastorska skala by Rikitan, Traditional Cache(2.3/3.8/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXY0CH</url>
        <urlname>Klastorska skala</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="864328">
            <groundspeak:name>Klastorska skala</groundspeak:name>
            <groundspeak:placed_by>Rikitan</groundspeak:placed_by>
            <groundspeak:owner id="5395">Rikitan</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
