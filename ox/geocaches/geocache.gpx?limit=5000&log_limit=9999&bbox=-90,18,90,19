        <urlname>Krzyzowa Góra</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="2407140">
            <groundspeak:name>Krzyzowa Góra</groundspeak:name>
