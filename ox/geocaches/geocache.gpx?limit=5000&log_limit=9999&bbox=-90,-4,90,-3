        <name>OX50</name>
        <desc>Dartmoor Tors - Birch Tor by Lord Darcy, Traditional Cache(2.1/2.6/3.1)</desc>
        <url>http://www.opencaching.com/#geocache/OX50</url>
        <urlname>Dartmoor Tors - Birch Tor</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="155">
            <groundspeak:name>Dartmoor Tors - Birch Tor</groundspeak:name>
            <groundspeak:placed_by>Lord Darcy</groundspeak:placed_by>
