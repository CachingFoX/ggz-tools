        <desc>Watery Wales 2 - Anglers Retreat by justal123, Traditional Cache(2.0/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX11DAQ</url>
        <urlname>Watery Wales 2 - Anglers Retreat</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="966138">
            <groundspeak:name>Watery Wales 2 - Anglers Retreat</groundspeak:name>
            <groundspeak:placed_by>justal123</groundspeak:placed_by>
            <groundspeak:owner id="57315">justal123</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
