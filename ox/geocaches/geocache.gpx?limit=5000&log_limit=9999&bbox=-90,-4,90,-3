        <name>OX35VK8</name>
        <desc>The Castle of Lawrence of India by Dartymoor, Traditional Cache(1.5/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX35VK8</url>
        <urlname>The Castle of Lawrence of India</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2945101">
            <groundspeak:name>The Castle of Lawrence of India</groundspeak:name>
            <groundspeak:placed_by>Dartymoor</groundspeak:placed_by>
            <groundspeak:owner id="56079">Dartymoor</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
