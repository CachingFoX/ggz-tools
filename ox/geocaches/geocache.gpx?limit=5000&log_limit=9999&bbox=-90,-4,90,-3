        <name>OX13GH7</name>
        <desc>Ystrad Fflur by justal123, Unknown Cache(2.9/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX13GH7</url>
        <urlname>Ystrad Fflur</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="1028804">
            <groundspeak:name>Ystrad Fflur</groundspeak:name>
            <groundspeak:placed_by>justal123</groundspeak:placed_by>
            <groundspeak:owner id="57315">justal123</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
