        <desc>EL SEXTO MOJO A LA MAIADA PEDREGOSA by ALAMOS_TRAIL, Traditional Cache(1.5/2.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTCJC</url>
        <urlname>EL SEXTO MOJO A LA MAIADA PEDREGOSA</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="28462507">
            <groundspeak:name>EL SEXTO MOJO A LA MAIADA PEDREGOSA</groundspeak:name>
            <groundspeak:placed_by>ALAMOS_TRAIL</groundspeak:placed_by>
            <groundspeak:owner id="123602">ALAMOS_TRAIL</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
