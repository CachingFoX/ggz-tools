        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2641286">
            <groundspeak:name>Bringing Brean To Life - Harry Cox</groundspeak:name>
            <groundspeak:placed_by>NT Brean Down</groundspeak:placed_by>
            <groundspeak:owner id="22739">NT Brean Down</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
