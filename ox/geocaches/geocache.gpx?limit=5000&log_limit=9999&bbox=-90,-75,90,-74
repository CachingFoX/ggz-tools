        <desc>Snow:  Spatial Dendrite by WEATHERNOWCAST, Traditional Cache(3.5/2.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX185GH</url>
        <urlname>Snow:  Spatial Dendrite</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1167167">
            <groundspeak:name>Snow:  Spatial Dendrite</groundspeak:name>
            <groundspeak:placed_by>WEATHERNOWCAST</groundspeak:placed_by>
            <groundspeak:owner id="7004">WEATHERNOWCAST</groundspeak:owner>
