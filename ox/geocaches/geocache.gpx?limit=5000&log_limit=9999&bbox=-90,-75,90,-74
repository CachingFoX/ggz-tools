        <desc>Snow Series:  Sleet by WEATHERNOWCAST, Traditional Cache(2.0/3.5/3.1)</desc>
        <url>http://www.opencaching.com/#geocache/OX186X6</url>
        <urlname>Snow Series:  Sleet</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1168489">
            <groundspeak:name>Snow Series:  Sleet</groundspeak:name>
            <groundspeak:placed_by>WEATHERNOWCAST</groundspeak:placed_by>
            <groundspeak:owner id="7004">WEATHERNOWCAST</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
