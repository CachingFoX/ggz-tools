        <name>OX277AC</name>
        <desc>Thompson Park Visitors Center by Ericles, Unknown Cache(2.0/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX277AC</url>
        <urlname>Thompson Park Visitors Center</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Unknown Cache</type>
        <groundspeak:cache archived="False" available="True" id="2062628">
            <groundspeak:name>Thompson Park Visitors Center</groundspeak:name>
            <groundspeak:placed_by>Ericles</groundspeak:placed_by>
            <groundspeak:owner id="6826">Ericles</groundspeak:owner>
            <groundspeak:type>Unknown Cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
