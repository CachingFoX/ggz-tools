        <name>OXZTJ5E</name>
        <desc>The Twins of Tatum by bobnjersey, Traditional Cache(3.0/3.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTJ5E</url>
        <urlname>The Twins of Tatum</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="28467872">
            <groundspeak:name>The Twins of Tatum</groundspeak:name>
            <groundspeak:placed_by>bobnjersey</groundspeak:placed_by>
            <groundspeak:owner id="38275">bobnjersey</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
