        <desc>The Pugnuts are Moving to Georgia by BigA800, Traditional Cache(2.3/1.8/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2GNGV</url>
        <urlname>The Pugnuts are Moving to Georgia</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2344401">
            <groundspeak:name>The Pugnuts are Moving to Georgia</groundspeak:name>
            <groundspeak:placed_by>BigA800</groundspeak:placed_by>
            <groundspeak:owner id="11363">BigA800</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
