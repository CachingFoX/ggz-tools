        <name>OX18W99</name>
        <desc>Pompton Mutiny: Commander Col. Israel Shreve by WEATHERNOWCAST, Traditional Cache(1.5/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX18W99</url>
        <urlname>Pompton Mutiny: Commander Col. Israel Shreve</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1188084">
            <groundspeak:name>Pompton Mutiny: Commander Col. Israel Shreve</groundspeak:name>
            <groundspeak:placed_by>WEATHERNOWCAST</groundspeak:placed_by>
            <groundspeak:owner id="7004">WEATHERNOWCAST</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
