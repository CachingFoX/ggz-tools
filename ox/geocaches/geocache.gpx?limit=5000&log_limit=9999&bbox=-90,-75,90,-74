        <groundspeak:cache archived="False" available="True" id="28471157">
            <groundspeak:name>Califon, NJ </groundspeak:name>
            <groundspeak:placed_by>colliesoftheborder</groundspeak:placed_by>
            <groundspeak:owner id="6190">colliesoftheborder</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
            <groundspeak:difficulty>2</groundspeak:difficulty>
