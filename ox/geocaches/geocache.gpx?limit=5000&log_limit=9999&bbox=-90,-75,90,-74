        <name>OXZTNYJ</name>
        <desc>Gateway to Apshawa by OLD-NAVY, Traditional Cache(2.8/1.8/2.7)</desc>
        <url>http://www.opencaching.com/#geocache/OXZTNYJ</url>
        <urlname>Gateway to Apshawa</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="28471503">
            <groundspeak:name>Gateway to Apshawa</groundspeak:name>
            <groundspeak:placed_by>OLD-NAVY</groundspeak:placed_by>
            <groundspeak:owner id="7005">OLD-NAVY</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
