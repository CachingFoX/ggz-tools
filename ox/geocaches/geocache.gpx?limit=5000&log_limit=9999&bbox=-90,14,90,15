        <groundspeak:cache archived="False" available="True" id="1766867">
            <groundspeak:name>Nelahozeves - kostel sv. Ondreje</groundspeak:name>
            <groundspeak:placed_by>ulmo</groundspeak:placed_by>
            <groundspeak:owner id="64679">ulmo</groundspeak:owner>
