        <name>OX1490P</name>
        <desc>HBL 7 Heilung by forever_chris, Multi-cache(1.0/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1490P</url>
        <urlname>HBL 7 Heilung</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="1051356">
            <groundspeak:name>HBL 7 Heilung</groundspeak:name>
            <groundspeak:placed_by>forever_chris</groundspeak:placed_by>
            <groundspeak:owner id="4536">forever_chris</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
