        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2675437">
            <groundspeak:name>die umWELTdenker 2011 Pfadfinder Traun- Oedt Ra/Ro</groundspeak:name>
            <groundspeak:placed_by>br3akstorm</groundspeak:placed_by>
            <groundspeak:owner id="40328">br3akstorm</groundspeak:owner>
