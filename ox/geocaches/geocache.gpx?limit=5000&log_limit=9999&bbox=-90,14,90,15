        <groundspeak:cache archived="False" available="True" id="28477055">
            <groundspeak:name>Rebensteiner Mauer</groundspeak:name>
            <groundspeak:placed_by>ENZEN</groundspeak:placed_by>
            <groundspeak:owner id="14244">ENZEN</groundspeak:owner>
