        <name>OX2956H</name>
        <desc>KHG Linz by da_Luigi, Traditional Cache(2.0/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2956H</url>
        <urlname>KHG Linz</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2120169">
            <groundspeak:name>KHG Linz</groundspeak:name>
            <groundspeak:placed_by>da_Luigi</groundspeak:placed_by>
            <groundspeak:owner id="96044">da_Luigi</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Micro</groundspeak:container>
            <groundspeak:difficulty>2</groundspeak:difficulty>
