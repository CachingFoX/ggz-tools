        <name>OX1490Y</name>
        <desc>HBL 8 Erholung by forever_chris, Multi-cache(2.0/2.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1490Y</url>
        <urlname>HBL 8 Erholung</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="1051363">
            <groundspeak:name>HBL 8 Erholung</groundspeak:name>
            <groundspeak:placed_by>forever_chris</groundspeak:placed_by>
            <groundspeak:owner id="4536">forever_chris</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
