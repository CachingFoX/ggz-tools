        <name>OX2V6AA</name>
        <desc>Geocaching na CVUT by DESTIL.CZ, Traditional Cache(1.5/1.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2V6AA</url>
        <urlname>Geocaching na CVUT</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2627694">
            <groundspeak:name>Geocaching na CVUT</groundspeak:name>
            <groundspeak:placed_by>DESTIL.CZ</groundspeak:placed_by>
            <groundspeak:owner id="20702">DESTIL.CZ</groundspeak:owner>
