        <desc>Skulpturlandskap Nordland Vågan by EWian, Traditional Cache(1.7/2.0/2.9)</desc>
        <url>http://www.opencaching.com/#geocache/OX349NQ</url>
        <urlname>Skulpturlandskap Nordland Vågan</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2899050">
            <groundspeak:name>Skulpturlandskap Nordland Vågan</groundspeak:name>
            <groundspeak:placed_by>EWian</groundspeak:placed_by>
            <groundspeak:owner id="62352">EWian</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
