        <name>OX2BJ4M</name>
        <desc>Radmerer Halsschmerzen by fam.feuerstein, Traditional Cache(1.5/3.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2BJ4M</url>
        <urlname>Radmerer Halsschmerzen</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="2192185">
            <groundspeak:name>Radmerer Halsschmerzen</groundspeak:name>
            <groundspeak:placed_by>fam.feuerstein</groundspeak:placed_by>
            <groundspeak:owner id="88786">fam.feuerstein</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
