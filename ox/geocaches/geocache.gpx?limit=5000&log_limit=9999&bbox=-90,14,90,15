        <name>OX2C4JM</name>
        <desc>Lichtenhainer Wasserfall Reloaded by DEEPDIVERBERLIN1, Multi-cache(2.0/3.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX2C4JM</url>
        <urlname>Lichtenhainer Wasserfall Reloaded</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Multi-cache</type>
        <groundspeak:cache archived="False" available="True" id="2208956">
            <groundspeak:name>Lichtenhainer Wasserfall Reloaded</groundspeak:name>
            <groundspeak:placed_by>DEEPDIVERBERLIN1</groundspeak:placed_by>
            <groundspeak:owner id="6785">DEEPDIVERBERLIN1</groundspeak:owner>
            <groundspeak:type>Multi-cache</groundspeak:type>
            <groundspeak:container>Regular</groundspeak:container>
            <groundspeak:difficulty>2</groundspeak:difficulty>
