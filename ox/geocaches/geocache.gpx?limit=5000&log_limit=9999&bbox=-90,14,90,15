        <name>OXHEQ7</name>
        <desc>The Ring of Clapham Junction by Gremxul, Traditional Cache(2.5/2.0/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OXHEQ7</url>
        <urlname>The Ring of Clapham Junction</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="520621">
            <groundspeak:name>The Ring of Clapham Junction</groundspeak:name>
            <groundspeak:placed_by>Gremxul</groundspeak:placed_by>
            <groundspeak:owner id="88923">Gremxul</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
