        <name>OX1WCKD</name>
        <desc>Golden Dreams by DJ&amp;JULES, Traditional Cache(1.5/2.5/3.0)</desc>
        <url>http://www.opencaching.com/#geocache/OX1WCKD</url>
        <urlname>Golden Dreams</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Traditional Cache</type>
        <groundspeak:cache archived="False" available="True" id="1740012">
            <groundspeak:name>Golden Dreams</groundspeak:name>
            <groundspeak:placed_by>DJ&amp;JULES</groundspeak:placed_by>
            <groundspeak:owner id="75476">DJ&amp;JULES</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
