        <groundspeak:cache archived="False" available="True" id="1009012">
            <groundspeak:name>Road to the Buffalo</groundspeak:name>
            <groundspeak:placed_by>-Orion-</groundspeak:placed_by>
            <groundspeak:owner id="4132">-Orion-</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
            <groundspeak:container>Small</groundspeak:container>
