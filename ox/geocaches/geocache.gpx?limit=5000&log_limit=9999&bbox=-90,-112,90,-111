        <groundspeak:cache archived="False" available="True" id="28478085">
            <groundspeak:name>Mountain Lion Tracking (read description)</groundspeak:name>
            <groundspeak:placed_by>Mindfulness</groundspeak:placed_by>
            <groundspeak:owner id="6864">Mindfulness</groundspeak:owner>
