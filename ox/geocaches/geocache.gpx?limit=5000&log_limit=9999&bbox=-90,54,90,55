        <groundspeak:cache archived="False" available="True" id="1581095">
            <groundspeak:name>Dirham Building</groundspeak:name>
            <groundspeak:placed_by>Carbon_Hunter</groundspeak:placed_by>
            <groundspeak:owner id="3134">Carbon_Hunter</groundspeak:owner>
            <groundspeak:type>Traditional Cache</groundspeak:type>
