        <urlname>City of the Future</urlname>
        <sym>Geocache</sym>
        <type>Geocache|Virtual Cache</type>
        <groundspeak:cache archived="False" available="True" id="28476818">
            <groundspeak:name>City of the Future</groundspeak:name>
            <groundspeak:placed_by>Carbon_Hunter</groundspeak:placed_by>
            <groundspeak:owner id="3134">Carbon_Hunter</groundspeak:owner>
